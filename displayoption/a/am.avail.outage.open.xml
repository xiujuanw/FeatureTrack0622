    <javascript.pre type="string">var id = vars['$L.filed']['outage.id'];
 
var incidentIdList = [];
lib.c.$('outageevent').setFields(['record.tag']).select('outage.id=\"'+id+'\"').iterate(function(item){
	var tag = item['record.tag'];
	var index = tag.indexOf('probsummary;');
	
	if(index&gt;=0){
		var id = tag.substring(12);
	}
	
	incidentIdList.push(id);
});



vars.$L_incidentIdList = incidentIdList;

vars.$L_sql = 'number isin $L.incidentIdList';


var incident = new SCFile('probsummary');
var rc = incident.doSelect(vars.$L_sql);
vars.$L_incident=incident;

if(RC_SUCCESS==rc) {
	vars['$L.view.type']=1;
} else {
	vars['$L.view.type']=0;
	system.functions.msg(system.functions.scmsg(137, "im"), 1);
}



