    <javascript.pre type="string">var teamId = vars.$L_file.teamId;

// check team owner first
var owned = lib.MicrosoftTeamsTeam.checkTeamOwned(teamId);
if (!owned) {
    funcs.rtecall("msg", new SCDatum(), funcs.scmsg("22", "msteams"), 3);
}
else {
    var result = lib.MicrosoftTeamsTeam.synchronizeTeam(teamId);
    var error = result.error;
    
    if (error) {
        var errorMsg = lib.MicrosoftTeamsUtil.errorMessage(error);
    
        if (error.code == lib.MicrosoftTeamsConstants.ERROR_CODE_NOTFOUND()) {
            errorMsg = funcs.scmsg("11", "msteams");
        }
    
        funcs.rtecall("msg", new SCDatum(), errorMsg, 3);
    }
    else {
        print(funcs.scmsg("5", "msteams"));
    }
}</javascript.pre>
