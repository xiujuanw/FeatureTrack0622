      <mappings sctype="structure">
        <smCallback type="string">/*
 *@reference No. 33061 requirement in QC
 */
if( $isCreate() ) { // This mechanism should only run on the creation of a new incident
	// 1)  The Business Service in the Event(1st priority)
	var device = $select( "device", 'logical.name="$sm.affected_item" and device.type="bizservice"' );
	if( device ) {
		$result = device['assignment'];
	}

	// 2)  The Category of the Incident(2nd priority)
	if( ! $result ) {
	    // retrieve category.file.name from object
	    var object = $select( "Object", 'file.name="probsummary"' );
	    if(object &amp;&amp; object['category.file.name'] &amp;&amp; object['category.file.name']=="category") {
		    // select default.assign from category only if PD4 is not enabled (it is using table category)
			var category = $select( "category", 'name="$sm.category"' );
			if( category ) {
				$result = category['default.assign'];
			}
	    }
	}

	// 3)  The CI in the Event(3rd priority)
	if( ! $result ) {
		var ci = $select( "device", 'logical.name="$sm.logical_name"' );
		if( ci ) {
			$result = ci['assignment'];
		}
	}

	// 4)  A default assignment group if no other criteria is met
	if( ! $result ) {
		$result = "Application";
	}

}</smCallback>
        <bdmObjectField NullValue="1" type="string"/>
