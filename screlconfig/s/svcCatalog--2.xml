<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="screlconfig">
  <record id="filename=&quot;svcCatalog&quot;" recordid="svcCatalog">
    <filename type="string">svcCatalog</filename>
    <query type="operator">"name isin "+str($L.unique.array)</query>
    <description.format type="string">screlate.catalog</description.format>
    <screen.id NullValue="1" type="string"/>
    <pre.open.expressions sctype="array">
      <pre.open.expressions sctype="operator">$L.pfilename=filename($L.primary)</pre.open.expressions>
      <pre.open.expressions sctype="operator">$L.process.names={"$L.pfilename", "$L.unique"}</pre.open.expressions>
      <pre.open.expressions sctype="operator">$L.process.values={$L.pfilename, $L.unique}</pre.open.expressions>
    </pre.open.expressions>
    <open.application type="string">se.call.process</open.application>
    <open.names sctype="array">
      <open.names sctype="string">file</open.names>
      <open.names sctype="string">name</open.names>
      <open.names sctype="string">names</open.names>
      <open.names sctype="string">values</open.names>
    </open.names>
    <open.values sctype="array">
      <open.values sctype="globalVar">$L.related</open.values>
      <open.values sctype="string">screlate.svcCatalog</open.values>
      <open.values sctype="globalVar">$L.process.names</open.values>
      <open.values sctype="globalVar">$L.process.values</open.values>
    </open.values>
    <post.open.expressions sctype="array">
      <post.open.expressions sctype="operator">if ($L.exit~="cancel") then (depend in $L.relation=str(name in $L.related))</post.open.expressions>
    </post.open.expressions>
    <pre.view.expressions sctype="array">
      <pre.view.expressions sctype="operator">if (filename($L.primary)="svcCatalog") then ($L.name=name in $L.primary)</pre.view.expressions>
      <pre.view.expressions sctype="operator">if (filename($L.related)="svcCatalog") then ($L.name=name in $L.related)</pre.view.expressions>
      <pre.view.expressions sctype="operator">$L.sql="name=\""+$L.name+"\""</pre.view.expressions>
    </pre.view.expressions>
    <view.application type="string">se.search.engine</view.application>
    <view.thread type="operator">gui()</view.thread>
    <view.names sctype="array">
      <view.names sctype="string">query</view.names>
      <view.names sctype="string">text</view.names>
    </view.names>
    <view.values sctype="array">
      <view.values sctype="globalVar">$L.sql</view.values>
      <view.values sctype="string">joinsvcDisplay</view.values>
    </view.values>
    <unique.fieldname type="string">name</unique.fieldname>
    <sysmodcount type="decimal">21</sysmodcount>
    <sysmoduser type="string">zhangqi</sysmoduser>
    <sysmodtime type="dateTime">07/13/16 20:51:58</sysmodtime>
    <desc.fields sctype="array">
      <desc.fields sctype="string">current.phase</desc.fields>
      <desc.fields sctype="string">status</desc.fields>
      <desc.fields sctype="string">description</desc.fields>
    </desc.fields>
    <active.cond type="boolean">true</active.cond>
    <common.name type="string">2777</common.name>
    <pre.open.process NullValue="1" type="string"/>
    <prompt.unrelate.reason NullValue="1" type="boolean"/>
    <prompt.relate.reason NullValue="1" type="boolean"/>
  </record>
</recordset>
