        <javascript.expression type="string">var $ = lib.c.$;

vars.$operVal = true;
vars.$memVal = true;
vars.$appVal = true;

var query = 'name="' + vars.$file.name + '" and ess.access.only#false and template.operator#false';
var operator = $("operator", SCFILE_READONLY).setFields("name").select(query).uniqueResult();
if (operator == null) {
    vars.$operVal = false;
}

var i, length, name, assignment;

// members
length = vars.$file.member_of.length();
for (i = 0; i &lt; length; i++) {
    name = vars.$file.member_of[i];
    assignment = $("assignment", SCFILE_READONLY).setFields("name").select('name="' + name + '"').uniqueResult();
    if (assignment == null) {
        vars.$memVal = false;
        vars.$memName = name;
        break;
    }
    else if (name != assignment.name) {
        // correct data in case insensitive db
        vars.$file.member_of[i] = assignment.name;
    }
}

// approvers
length = vars.$file.approver_of.length();
for (i = 0; i &lt; length; i++) {
    name = vars.$file.approver_of[i];
    assignment = $("assignment", SCFILE_READONLY).setFields("name").select('name="' + name + '"').uniqueResult();
    if (assignment == null) {
        vars.$appVal = false;
        vars.$appName = name;
        break;
    }
    else if (name != assignment.name) {
        // correct data in case insensitive db
        vars.$file.approver_of[i] = assignment.name;
    }
}</javascript.expression>
      </javascript>
