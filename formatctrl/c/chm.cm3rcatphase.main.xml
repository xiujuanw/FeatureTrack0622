        <javascript.delete NullValue="1" type="boolean"/>
        <javascript.display NullValue="1" type="boolean"/>
        <javascript.init NullValue="1" type="boolean"/>
        <javascript.expression type="string">
	vars.$catisnull = false;
	/** firstly, cleanup all the empty rows */
	var matrix = cleanupEmptyRow([vars.$file.auto_task_cat, vars.$file.auto_task_opn, vars.$file.auto_task_blind, vars.$file.auto_task_each_member]);
	
	vars.$file.auto_task_cat = new SCDatum(matrix[0]);
	vars.$file.auto_task_opn = new SCDatum(matrix[1]);
	vars.$file.auto_task_blind = new SCDatum(matrix[2]);
	vars.$file.auto_task_each_member = new SCDatum(matrix[3]);
	
	/** secondly, find if there's any empty category in the phase, if yes, pop the error to user. */
	for(var i = 0; i &lt; vars.$file.auto_task_cat.length(); i++){
		if(isEmpty(vars.$file.auto_task_cat[i])){
			vars.$catisnull = true;
			break;
		}
	}

	/**
	 * The function is used for cleanup all the empty rows. 
	 * Parameter matrix should be a 2-D array, no column length limit or row length limit.
	 * Since the elements of the matrix will be re-located, so a new matrix will be returned.
	 * Be sure to use the returned matrix.
	 */
	function cleanupEmptyRow(matrix){
		for(var i = 0; i &lt; matrix[0].length(); i++){
			if(isEmpty(matrix[0][i])){
				var empty = true;
				for(var j = 1; j &lt; matrix.length; j++){
					if(!isEmpty(matrix[j][i])){
						empty = false;
						break;
					}
				}
				if(empty){
					for(var j = 0; j &lt; matrix.length; j++){
						matrix[j] = system.functions._delete(matrix[j], i + 1);
					}
					i--;
				}
			}
		}
		return matrix;
	}
	
	function isEmpty(element){
		if(element === null || element === ""){
			return true;
		}
		return false;
	}</javascript.expression>
      </javascript>
    </javascript>
