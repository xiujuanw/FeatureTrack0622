        <javascript.expression type="string">var removeNullAndDuplicate=lib.tableUtil.removeNullAndDuplicate;
var _null=system.functions._null;
var scmsg=system.functions.scmsg;
var setCursorField=system.functions.cursor_field_name_set;
var dFile;
var query;
vars.$invalid=false;
vars.$invalidMsg="";
vars.$cursorField="";
if(!_null(vars['$file']['supported.companies'])){
	for(var i=0;i&lt;vars['$file']['supported.companies'].length();i++){
		var value=vars['$file']['supported.companies'][i];
		if(!_null(value) &amp;&amp; value!==""){
			dFile=new SCFile("company");
			query="company=\""+value+"\"";
			if(dFile.doSelect(query)!==RC_SUCCESS){
				vars.$cursorField="supported.companies";
		    	setCursorField("supported.companies",(i+1));
				vars.$invalid=true;
				vars.$invalidMsg=scmsg(3023, "fc", ["'"+value+"'"]);
				break;
			}
		}
	}
}

if(!_null(vars['$file']['supported.locations'])){
	for(var i=0;i&lt;vars['$file']['supported.locations'].length();i++){
		var value=vars['$file']['supported.locations'][i];
		if(!_null(value) &amp;&amp; value!==""){
			dFile=new SCFile("location");
			query="location=\""+value+"\"";
			if(dFile.doSelect(query)!==RC_SUCCESS){
				vars.$cursorField="supported.locations";
		    	setCursorField("supported.locations",(i+1));
				vars.$invalid=true;
				vars.$invalidMsg=scmsg(3022, "fc", ["'"+value+"'"]);
				break;
			}
		}
	}
}
if(!_null(vars['$file']['supported.depts'])){
	for(var i=0;i&lt;vars['$file']['supported.depts'].length();i++){
		var value=vars['$file']['supported.depts'][i];
		if(!_null(value) &amp;&amp; value!==""){
			dFile=new SCFile("dept");
			query="dept=\""+value+"\"";
			if(dFile.doSelect(query)!==RC_SUCCESS){
				vars.$cursorField="supported.depts";
		    	setCursorField("supported.depts",(i+1));
				vars.$invalid=true;
				vars.$invalidMsg=scmsg(3021, "fc", ["'"+value+"'"]);
				break;
			}
		}
	}
}
vars['$file']['supported.depts']=removeNullAndDuplicate(vars['$file'],vars['$file']['supported.depts']);
vars['$file']['supported.languages']=removeNullAndDuplicate(vars['$file'],vars['$file']['supported.languages']);
vars['$file']['supported.locations']=removeNullAndDuplicate(vars['$file'],vars['$file']['supported.locations']);
vars['$file']['supported.companies']=removeNullAndDuplicate(vars['$file'],vars['$file']['supported.companies']);
</javascript.expression>
