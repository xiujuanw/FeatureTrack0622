        <ruleJSON type="string">{"type":"popupMessageBox","popupMessage":"","popupType":1,"okLevel":1,"okType":1,"yesnoType":1,"yesnocancelType":1,"messageNumber":"","messageScript":"message=system.functions.scmsg(\"releaseBuild\",\"featuretracker\",[vars.$featureList]);"}</ruleJSON>
