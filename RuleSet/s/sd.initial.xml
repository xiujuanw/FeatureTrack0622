        <condition type="operator">not  (same(delay.call.number in $G.sm.global.environment, true))</condition>
        <conditionDesc type="string">( Expression: not same(delay.call.number in $G.sm.global.environment, true))</conditionDesc>
        <conditionXML type="string">&lt;XmlCondition&gt;&lt;collection logical="" negate="" &gt;&lt;condition logical="" negate="false"&gt;&lt;operand type="expression"&gt;&lt;Expression caption=""&gt;"not same(delay.call.number in $G.sm.global.environment, true)"&lt;/Expression&gt;&lt;/operand&gt;&lt;/condition&gt;&lt;/collection&gt;&lt;/XmlCondition&gt;</conditionXML>
        <fullDesc type="string">Set Interaction ID using sdInteraction number record (when ( Expression: not same(delay.call.number in $G.sm.global.environment, true)))</fullDesc>
        <level type="decimal">0</level>
        <type type="string">setNumber</type>
        <ruleJSON type="string">{"type":"setNumber","fieldName":"incident.id","numberRecord":"incidents","overwrite":"null"}</ruleJSON>
      </rule>
      <rule sctype="structure">
        <ruleId type="decimal">7</ruleId>
