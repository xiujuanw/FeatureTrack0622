<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="RuleSet">
  <record id="id=&quot;sd.initial.ess&quot;" recordid="sd.initial.ess">
    <id type="string">sd.initial.ess</id>
    <name type="string">Initial the Interaction Record in ESS env</name>
    <tablename sctype="array">
      <tablename sctype="string">incidents</tablename>
    </tablename>
    <rule sctype="array">
      <rule sctype="structure">
        <ruleId type="decimal">5</ruleId>
        <isGroup type="boolean">false</isGroup>
        <parentGroup type="decimal">0</parentGroup>
        <ruleXML NullValue="1" type="string"/>
        <ruleDesc type="string">Set ess.entry</ruleDesc>
        <condition type="operator">$G.ess="true" and current.phase in $L.file="Logging"</condition>
        <conditionDesc type="string">( $G.ess = "true" AND Phase in CurrentRecord = "Logging")</conditionDesc>
        <conditionXML type="string">&lt;XmlCondition&gt;&lt;collection logical="" negate=""&gt;&lt;condition logical="" negate="false"&gt;&lt;operand type="variable"&gt;&lt;VariableName caption="$G.ess"&gt;$G.ess&lt;/VariableName&gt;&lt;/operand&gt;&lt;Comparison&gt;equals&lt;/Comparison&gt;&lt;Value dataType="string"&gt;true&lt;/Value&gt;&lt;/condition&gt;&lt;condition logical="AND" negate="false"&gt;&lt;operand type="field"&gt;&lt;TableName&gt;CurrentRecord&lt;/TableName&gt;&lt;FieldName caption="Phase"&gt;current.phase&lt;/FieldName&gt;&lt;/operand&gt;&lt;Comparison&gt;equals&lt;/Comparison&gt;&lt;Value dataType=""&gt;Logging&lt;/Value&gt;&lt;/condition&gt;&lt;/collection&gt;&lt;/XmlCondition&gt;</conditionXML>
        <fullDesc type="string">Set ess.entry (when ( $G.ess = "true" AND Phase in CurrentRecord = "Logging"))</fullDesc>
        <level type="decimal">0</level>
        <type type="string">JSValidate</type>
        <ruleJSON type="string">{"type":"JSValidate","script":"record[\"ess.entry\"] = true;"}</ruleJSON>
      </rule>
      <rule sctype="structure">
        <ruleId type="decimal">2</ruleId>
        <isGroup type="boolean">false</isGroup>
        <parentGroup type="decimal">0</parentGroup>
        <ruleXML NullValue="1" type="string"/>
        <ruleDesc type="string">Init contact name</ruleDesc>
        <condition type="operator">current.phase in $L.file="Logging"</condition>
        <conditionDesc type="string">( Phase in CurrentRecord = "Logging")</conditionDesc>
        <conditionXML type="string">&lt;XmlCondition&gt;&lt;collection logical="" negate=""&gt;&lt;condition logical="" negate="false"&gt;&lt;operand type="field"&gt;&lt;TableName&gt;CurrentRecord&lt;/TableName&gt;&lt;FieldName caption="Phase"&gt;current.phase&lt;/FieldName&gt;&lt;/operand&gt;&lt;Comparison&gt;equals&lt;/Comparison&gt;&lt;Value dataType=""&gt;Logging&lt;/Value&gt;&lt;/condition&gt;&lt;/collection&gt;&lt;/XmlCondition&gt;</conditionXML>
        <fullDesc type="string">Init contact name (when ( Phase in CurrentRecord = "Logging"))</fullDesc>
        <level type="decimal">0</level>
        <type type="string">JSValidate</type>
        <ruleJSON type="string">{"type":"JSValidate","script":"// Modified so that this logic is skipped entirely when not in ESS mode.\u000a// Otherwise it caused an unneeded select statement.\u000aif (vars.$G_ess == true &amp;&amp; vars.$L_file.contact_name == null)\u000a{\u000a  var fContacts = new SCFile(\"contacts\");\u000a  var strContactName=\"\";\u000a \u000a  if ( typeof fContacts == \"object\" &amp;&amp; fContacts.doSelect != undefined ) \u000a  {\u000a    var rc = fContacts.doSelect(\"operator.id=\\\"\"+ system.functions.operator()+\"\\\"\")\u000a    if ( fContacts != null )\u000a    {\u000a       strContactName=fContacts.contact_name;\u000a       vars.$L_file.contact_name = strContactName;\u000a       vars.$lo_contact_name = strContactName; \u000a    }\u000a  }\u000a}\u000a"}</ruleJSON>
      </rule>
      <rule sctype="structure">
        <ruleId type="decimal">4</ruleId>
        <isGroup type="boolean">false</isGroup>
        <parentGroup type="decimal">0</parentGroup>
        <ruleXML NullValue="1" type="string"/>
        <ruleDesc type="string">Call the "sd.SubscriptionItem" process.</ruleDesc>
        <condition type="operator">$G.ess="true"</condition>
        <conditionDesc type="string">( $G.ess = "true")</conditionDesc>
        <conditionXML type="string">&lt;XmlCondition&gt;&lt;collection logical="" negate=""&gt;&lt;condition logical="" negate="false"&gt;&lt;operand type="variable"&gt;&lt;VariableName caption="$G.ess"&gt;$G.ess&lt;/VariableName&gt;&lt;/operand&gt;&lt;Comparison&gt;equals&lt;/Comparison&gt;&lt;Value dataType="string"&gt;true&lt;/Value&gt;&lt;/condition&gt;&lt;/collection&gt;&lt;/XmlCondition&gt;</conditionXML>
        <fullDesc type="string">Call the "sd.SubscriptionItem" process. (when ( $G.ess = "true"))</fullDesc>
        <level type="decimal">0</level>
        <type type="string">callProcess</type>
        <ruleJSON type="string">{"type":"callProcess","process":"sd.SubscriptionItem"}</ruleJSON>
      </rule>
    </rule>
    <lastRuleId type="decimal">5</lastRuleId>
