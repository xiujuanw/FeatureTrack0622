    <javascript.actions type="string">	var file=new SCFile("scversion");
	var rc=file.doSelect("true");
	if(rc == RC_SUCCESS){
		vars.$L_file.current_version=file.application_version;
		
		var allfiles=lib.bzfilesPurge.getCustomerAllFiles();
		var oob=lib.bzfilesPurge.getSysOOBFiles();
		var upgradefiles=lib.bzfilesPurge.getSysUpgradeRequiredFiles();
		var upgradeGenfiles=lib.bzfilesPurge.getSysUpgradeGeneratedFiles();
		var systemfiles=lib.bzfilesPurge.getSysSystemFiles();
		//print(systemfiles.length, upgradeGenfiles.length, upgradefiles.length);
		var defaultfiles=lib.bzfilesPurge.getSysDefaultBZFiles();
		vars.$L_file.bz_default_files=defaultfiles;
		
		var defaultBzfiles=defaultfiles;
		
		var limitFiles=upgradefiles.concat(upgradeGenfiles).concat(systemfiles);
		//print("limit files:", limitFiles.length);
		
		//print(oob.length());
		//print(defaultBzfiles.length());
		//print(upgrade.length());
		vars.$oob_part_files=lib.bzfilesPurge.getDiffsets(oob, defaultBzfiles, limitFiles);
		//print(vars.$oob_part_files.length());
		vars.$custom_files=lib.bzfilesPurge.getDiffsets(allfiles, oob, []);
		//print(vars.$custom_files.length());
	}
	
	
	
