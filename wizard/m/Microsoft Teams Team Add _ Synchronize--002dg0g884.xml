    <javascript.actions type="string">// get selected team's ids
var rows = funcs.multiselect_selection("rows");
if (rows) {
    var length = rows.length();
    var i;
    for (i = 0; i &lt; length; i++) {
        var row = rows[i];
        vars.$team_ids_select.push(vars.$L_team_ids[row-1]);
    }
}</javascript.actions>
    <javascript.init type="string">vars.$L_team_ids = [];
vars.$L_team_names = [];
vars.$team_ids_select = [];

// retrieve teams list to create
var result = lib.MicrosoftTeamsTeam.listOwnedTeamsToSynchronize(true);
if (result.error) {
    funcs.rtecall("msg", new SCDatum(), lib.MicrosoftTeamsUtil.errorMessage(result.error), 3);
}
else {
    var teams = result.teams;

    teams.sort(function(a, b) {
        return (a.displayName ? a.displayName.localeCompare(b.displayName) : -1);
    });
    
    teams.forEach(function(team) {
        vars.$L_team_ids.push(team.id);
        vars.$L_team_names.push(team.displayName);
    });
}</javascript.init>
