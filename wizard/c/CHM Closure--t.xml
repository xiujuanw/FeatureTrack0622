    <javascript.init type="string">vars.$cm_close_codes_cfd=vars.$G_cm_close_codes;
vars.$cm_close_codes_desc_cfd=vars.$G_cm_close_codes_desc;
if(vars.$L_file.current_phase=="Abandoned")
{
		var close_code = vars.$cm_close_codes_cfd.toArray();
        var close_code_dis = vars.$cm_close_codes_desc_cfd.toArray();
        var close_code_1 = new Array();
        var close_code_dis_1 = new Array();
        for (var i=0; i&lt;= vars.$cm_close_codes_cfd.length() ;i++)
        {
                if (close_code[i]==4|| close_code[i]==5||close_code[i]==6)
                {
                        close_code_1.push(close_code[i]);
                        close_code_dis_1.push(close_code_dis[i]);
                }
        }
        vars.$cm_close_codes_cfd=lib.ArrayUtil.toSCDatum(close_code_1);
        vars.$cm_close_codes_desc_cfd=lib.ArrayUtil.toSCDatum(close_code_dis_1);
        if(vars.$L_file.completion_code!=4&amp;&amp;vars.$L_file.completion_code!=5&amp;&amp;vars.$L_file.completion_code!=6)
        {
            vars.$L_file.completion_code=null;

        }
}</javascript.init>
