    <javascript.init type="string">vars.$L_l10n_support = false;

var txtpath = vars.$upgrade_path + "AppUpgVersion.txt";

var content_info = lib.ContentPackHelper.getContentInfo(txtpath);
if (content_info != null) {
	vars.$release_type="Content";
	vars.$release_type_title="Content Patch";
	
	// determine whether support multiple languanges
	vars.$L_l10n_support = ( "true" == content_info.l10nsupport ) ? true : false;	
}
else {
	vars.$release_type="Patch";
	vars.$release_type_title="Normal Patch";
}

var index = vars.$patchrel_path.indexOf("patchrel.unl");
if (index &gt; 0) {
	vars.$patchrel_path = vars.$patchrel_path.substring(0, index);
}
</javascript.init>
