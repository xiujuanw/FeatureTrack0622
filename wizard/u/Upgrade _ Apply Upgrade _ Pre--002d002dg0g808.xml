    <javascript.init type="string">vars.$L_invalid = false;

// upgrade status record
vars.$L_status_rec = lib.upgradeUtility.getRecord("upgradestatus", "version=\"" + lib.upgradeConstants.TARGET_VERSION() + "\"");

// whether upgrade already in progress  
vars.$L_upgrade_inprogress = false;

if (system.functions.sysinfo_get("mode") == "client") {
    vars.$L_invalid = true;
    vars.$invalid_msg = "The Service Manager Upgrade may not be run from a standard client. Please use an express or SM client.";
}
else {
    var currentStep = 0;
    if (vars.$L_status_rec != null &amp;&amp; vars.$L_status_rec["current.step"] != null) {
   		currentStep = vars.$L_status_rec["current.step"];
    }
    	    
    if (vars.$L_status_rec != null &amp;&amp; vars.$L_status_rec["transfer.loaded"] == true &amp;&amp; currentStep &lt;= 99) {
		vars.$L_upgrade_inprogress = true;
    }

	var hasRenamedRecord = lib.upgradeHelper.hasRenamedRecord();
	
	if (hasRenamedRecord) {
//	    var checkRename = currentStep &lt; 8 || currentStep &gt; 99;
	    var checkRename = currentStep &lt; 8;
	    if (checkRename) {
	        vars.$L_invalid = true;
	        vars.$invalid_msg="Some conflicts that are reported in the Upgrade Results list have not been resolved. Please resolve the conflicts first.";
	    }
	}
}

</javascript.init>
