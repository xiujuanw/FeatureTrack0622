    <javascript.actions type="string">if(vars.$labelmode=="istext"){
	vars.$result = lib.SRCConfigWizard.insertLabel(vars.$newlabelvalue, vars.$lang);
	if (vars.$result == false)
		return;
		
	vars.$labelid=vars.$result;
}
vars.$result = true;

var checkbox = new lib.SrcConfigXML.Checkbox;
checkbox.setField(vars.$field);
checkbox.setLabelId(vars.$labelid);
checkbox.setModifiability(vars.$modifiability);

  
if (vars.$hasmandatory &amp;&amp; (vars.$mandatory == null || vars.$mandatory == "") )
   vars.$mandatory = true;
else if (!vars.$hasmandatory) vars.$mandatory = false;

if (vars.$hasvisible &amp;&amp; (vars.$visible == null || vars.$visible == "") )
  vars.$visible = true;
else if (!vars.$hasvisible) vars.$visible = false;

  
checkbox.setMandatory(vars.$mandatory);
checkbox.setVisible(vars.$visible);

if (vars.$hasdefaultvalue) {
	checkbox.setDefaultValue(vars.$defaultvalue);
	if (vars.$useExpression) {
		checkbox.setUseExpression(vars.$useExpression);
		checkbox.setDefaultValue(vars.$defaultvalueexp);
	}
}
	
var job = new lib.SRCConfigWizardJobs.AddFieldJob;
job.setArtifact(checkbox);
job.run();
</javascript.actions>
