    <javascript.init type="string">var baselineSql = "name=\""+system.vars.$selected_baseline+"\" and version=\""+system.vars.$selected_version+"\"";
var baseline = new SCFile("baseline");
var rc = baseline.doSelect(baselineSql);


//filter the field list tha can only be used for selected CI device type. 
var commonFieldNameList = lib.GetFieldNames.getAllFieldsForExpState(system.vars.$this_filename)[0].toArray(); 
	
for (var i = 0 ; i &lt;system.functions.lng(baseline.field_names); i++ ){

	 if(lib.ArrayUtil.contains(commonFieldNameList,baseline.field_names[i])){
 	 	system.vars.$field_name=system.functions.insert(system.vars.$field_name,1,1,baseline.field_names[i]);
 	 	system.vars.$field_value=system.functions.insert(system.vars.$field_value,1,1,baseline.field_values[i]);
 	 	
 	 }
}

vars['$field.value.display']=lib.ddmEventHelper.makeDisplayValueList(vars['$this.filename'], vars['$field.name'].toArray(), vars['$field.value'].toArray());
