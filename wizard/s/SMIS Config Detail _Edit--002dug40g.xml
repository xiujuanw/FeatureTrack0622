    <javascript.init type="string">if (vars.$L_instanceFile == null) {
	var fieldMapping = new SCFile("SMISFieldMapping");
	fieldMapping.doSelect("intId=\"" + vars.$L_file.intId + "\"");
	vars.$L_mappingFile = fieldMapping;

	//sync custom param format
	var registry = new SCFile("SMISRegistry");
	registry.doSelect("name=\"" + vars.$L_file.template + "\"");
	vars.$L_file.custom_param_format = registry.custom_param_format;
	vars.$L_instanceFile = vars.$L_file;
}

//set the model by the instance status
if (vars.$L_file.status == lib.smis_Constants.INSTANCE_STATUS_DISABLE()) {
	vars.$L_editModel = true;
	vars.$L_viewModel = false;
} else {
	vars.$L_editModel = false;
	vars.$L_viewModel = true;
}

if(vars.$L_viewModel == false &amp;&amp; vars.$L_param_previous != null &amp;&amp; vars.$L_param_previous  == "fromPD") {
	
	if (vars.$L_file.custom_param_format != null &amp;&amp; vars.$L_file.custom_param_format != "") {
		lib.smis_ConfigurationManager.populateParamFromVariables(vars.$L_file);
	} else {
		vars.$L_file.paramInfo = lib.smis_ConfigurationManager.populateParamInfo("SMISConfiguration");
	}
	//vars.$L_file.paramInfo = lib.smis_ConfigurationManager.populateParamInfo("SMISConfiguration");
	
}

// merge the paraminfo from registry without default value
if(vars.$L_viewModel == false &amp;&amp; vars.$L_file.custom_param_format != null &amp;&amp; vars.$L_file.custom_param_format != "") {
    lib.smis_ConfigurationManager.mergeParamInfoFromRegistry(vars.$L_file, registry);
}

vars.$L_param_showed = false;
