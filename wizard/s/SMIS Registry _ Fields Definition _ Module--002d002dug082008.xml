    <javascript.init type="string">var variables = lib.smis_FieldMappingManager.getFieldVariables();
var lng2 = variables.length;

var i, j;
var splitVars = [];
for (i=0; i&lt;lng2; i++) {
    vars[variables[i]] = [];
    splitVars.push(lib.smis_FieldMappingManager.getSplitThreadVariable(variables[i]));
}
vars["$SMFields.SMModuleName"] = [];

var lng = vars["$L.file"]["SMFields"] == null ? 0 : funcs.lng(vars["$L.file"]["SMFields"]);
var splitVar;
for (i=0; i&lt;lng; i++) {
    if (vars["$smis.selected.module"] == vars["$L.file"]["SMFields"][i]["SMModuleName"]) {
        for (j=0; j&lt;lng2; j++) {
            splitVar = splitVars[j];
            vars[variables[j]].push(vars["$L.file"][splitVar[0]][i][splitVar[1]]);
        }
        vars["$SMFields.SMModuleName"].push(vars["$L.file"]["SMFields"][i]["SMModuleName"]);
    }
}</javascript.init>
