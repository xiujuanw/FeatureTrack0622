    <javascript.actions type="string">var ts = new lib.GatewayAPI_PortalFormConfigWizard.TableSelection('$sectionIDs');
vars['$selection'] = ts.getSelectedIndex();
if (vars['$selection'] != 0) {
	vars['$sectionID'] = vars['$sectionIDs'][vars['$selection'] - 1];
	vars['$section.index'] = vars['$selection'] - 1;
}

if (vars['$L.option'] == 407 || vars['$L.option'] == 408) {
	vars['$restart'] = true;
	var situation = new lib.GatewayAPI_PortalFormConfigWizardJobs.Situation();
	var job = new lib.GatewayAPI_PortalFormConfigWizardJobs.MoveSectionJob(), direction;
	if (vars['$L.option'] == 407) { // move up
		direction = -1;
	} else if (vars['$L.option'] == 408) { //move down
		direction = 1;
	}
	job.setMoveDirection(direction);
	job.run();
	
	ts.setSelectedIndex(vars['$selection'] + direction);
	
	vars['$sectionIDs'] = lib.GatewayAPI_PortalFormConfigWizard.getSectionIDs(vars['$options']);
	vars['$sections'] = lib.GatewayAPI_PortalFormConfigWizard.getLabels(vars['$sectionIDs'], vars['$lang']);
}

lib.GatewayAPI_PortalFormConfigWizard.clearFieldVars();</javascript.actions>
    <javascript.init type="string">if (!vars.$options) {
	vars['$L.file']=lib.GatewayAPI_PortalFormConfigWizard.getConfByName(vars.$confName);
	vars.$options=vars['$L.file'].options;
}
vars.$sectionIDs=lib.GatewayAPI_PortalFormConfigWizard.getSectionIDs(vars.$options);
vars.$sections=lib.GatewayAPI_PortalFormConfigWizard.getLabels(vars.$sectionIDs, vars.$lang);


var ts = new lib.GatewayAPI_PortalFormConfigWizard.TableSelection('$sectionIDs');
ts.fixSelectedIndex();</javascript.init>
