if(vars.$L_viewModel == false &amp;&amp; vars.$L_param_showed == true &amp;&amp; vars.$L_param_populated == false) {
	if (vars.$L_file.custom_param_format != null &amp;&amp; vars.$L_file.custom_param_format != "") {
