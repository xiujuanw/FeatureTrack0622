      <init.expressions sctype="operator">if (created.from.wiz in $L.file=true and index("quantity", str(condition in $L.file))&gt;0 and index("&lt;=", str(condition in $L.file))=0 and index("&gt;=", str(condition in $L.file))=0) then ($stop=false;$begin=index("quantity", str(condition in $L.file))+20;$lng=lng(str(condition in $L.file))+1;$compare=substr(str(condition in $L.file), $begin - 1, 1);for $i = $begin to $lng do (if ($stop=false and substr(str(condition in $L.file), $i, 1)=" ") then ($stop=true;$end=$i));$slo.quantity=substr(str(condition in $L.file), $begin, $end - $begin))</init.expressions>
      <init.expressions sctype="operator">if (created.from.wiz in $L.file=true and index("quantity", str(condition in $L.file))&gt;0 and (index("&lt;=", str(condition in $L.file))&gt;0 or index("&gt;=", str(condition in $L.file))&gt;0)) then ($stop=false;$begin=index("quantity", str(condition in $L.file))+21;$lng=lng(str(condition in $L.file))+1;$compare=substr(str(condition in $L.file), $begin - 2, 2);for $i = $begin to $lng do (if ($stop=false and substr(str(condition in $L.file), $i, 1)=" ") then ($stop=true;$end=$i));$slo.quantity=substr(str(condition in $L.file), $begin, $end - $begin))</init.expressions>
