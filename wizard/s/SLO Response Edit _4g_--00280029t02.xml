      <init.expressions sctype="operator">if (created.from.wiz in $L.file=true and index("item.description", str(condition in $L.file))&gt;0) then ($stop=false;$begin=index("item.description", str(condition in $L.file))+29;$lng=lng(str(condition in $L.file))+1;for $i = $begin to $lng do (if ($stop=false and substr(str(condition in $L.file), $i, 1)="\"") then ($stop=true;$end=$i));$slo.item.description=substr(str(condition in $L.file), $begin, $end - $begin))</init.expressions>
