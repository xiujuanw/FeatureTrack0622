    <javascript.init type="string">var number = vars.$L_file.number;
vars.$L_file = new SCFile('cm3r');
vars.$L_file.number = number;

vars["$dme.mode.values"]=vars["$dme.mode.values"] == null ? lib.ddmEventUtil.getDmeModeValueList() : vars["$dme.mode.values"];
vars["$dme.mode.values.display"]=vars["$dme.mode.values.display"] == null ? lib.ddmEventUtil.getDmeModeDisplayValueList() : vars["$dme.mode.values.display"];

if (vars["$field.name"] != null) {
    var structModes = {};
    if (vars["$dme.mode"] == null) {
        vars["$dme.mode"] = [];
    }
    vars["$dme.mode"] = funcs.denull(vars["$dme.mode"]);
    var modeLength = funcs.lng(vars["$dme.mode"]);
    vars["$field.name"] = funcs.denull(vars["$field.name"]);
    var length = funcs.lng(vars["$field.name"]);

    for (var i=0; i&lt;modeLength &amp;&amp; i&lt;length; i++) {
        var struct = lib.expectedStateHelper.getStructName(vars["$this.filename"],vars["$field.name"][i]);
        if (vars["$dme.mode"][i] != null) {
            if (struct == null || structModes[struct] == null) {
                structModes[struct] = vars["$dme.mode"][i];
            }
        } else {
            if (struct == null || structModes[struct] == null) {
                vars["$dme.mode"][i] = lib.ddmEventUtil.getDmeModeReplace();
                structModes[struct] = vars["$dme.mode"][i];
            } else {
                vars["$dme.mode"][i] = structModes[struct];
            }
        }
    }
    
    for (var i=modeLength; i&lt;length; i++) {
        var struct = lib.expectedStateHelper.getStructName(vars["$this.filename"],vars["$field.name"][i]);
        if (struct == null || structModes[struct] == null) {
            vars["$dme.mode"].push(lib.ddmEventUtil.getDmeModeReplace());
            structModes[struct] = vars["$dme.mode"][i];
        } else {
            vars["$dme.mode"].push(structModes[struct]);
        }
    }
    
    for (var i=modeLength; i&gt;=length; i--) {
        vars["$dme.mode"]=funcs._delete(vars["$dme.mode"], i+1);
    }
}</javascript.init>
