    <javascript.actions type="string">var jsonString = vars.$L_file.ruleJSON;
if(null==jsonString || ""==jsonString) {// keep backward compatibility to use rule XML
	var xmlString;
	xmlString = vars.$L_file.ruleXML;
	
	if ( xmlString == null || xmlString =="" )
		{
			xmlString="&lt;callWizard&gt;&lt;/wizardName&gt;&lt;/callWizard&gt;";
		}
	
	
	var xml = new XML();
	xml.setContent( xmlString );
	var wizName = lib.xmlHelpers.getElement( xml, "wizardName", null );
	
	if (wizName != null) {vars.$L_ruleWizardName = wizName.getValue() };
} else {
    var JSON = system.library.JSON.json();
    var jsonObj = JSON.parse(jsonString);
    var wizName = jsonObj.wizardName;
    if(wizName != null) {
        vars.$L_ruleWizardName = wizName;
    } else {
        vars.$L_ruleWizardName = "";
    }
}</javascript.actions>
