      <statements sctype="operator">$L.min.current.seq=3000</statements>
      <statements sctype="string"/>
      <statements sctype="operator">for $L.i = 1 to lng(denull($L.total.approvals)) do ($L.current.seq=current.pending.seq in $L.i in $L.total.approvals;if ($L.min.current.seq&gt;$L.current.seq) then ($L.min.current.seq=$L.current.seq;$L.min.index=$L.i))</statements>
      <statements sctype="string"/>
      <statements sctype="operator">$L.rem="next, look into current.pending.groups, if the seq is bigger than min seq, then move current.pending.groups to future.pending.groups, also update the future seq number"</statements>
      <statements sctype="string"/>
      <statements sctype="operator">$L.rem="after move current pending groups to future, set that element to empty so that the original array order is maintained"</statements>
      <statements sctype="string"/>
      <statements sctype="operator">for $L.i = 1 to lng(denull($L.total.approvals)) do (if (current.pending.seq in $L.i in $L.total.approvals~=$L.min.current.seq) then for $L.j = 1 to lng(denull(current.pending.groups in $L.i in $L.total.approvals)) do (future.groups in $L.i in $L.total.approvals=insert(future.groups in $L.i in $L.total.approvals, 1, 1, $L.j in current.pending.groups in $L.i in $L.total.approvals);future.seq in $L.i in $L.total.approvals=insert(future.seq in $L.i in $L.total.approvals, 1, 1, current.pending.seq in $L.i in $L.total.approvals);$L.j in current.pending.groups in $L.i in $L.total.approvals=""))</statements>
      <statements sctype="string"/>
      <statements sctype="operator">$L.rem="remove empty ones from future groups"</statements>
      <statements sctype="string"/>
      <statements sctype="operator">for $L.i = 1 to lng(denull($L.total.approvals)) do for $L.j = 1 to lng(current.pending.groups in $L.i in $L.total.approvals) do (if (lng($L.j in current.pending.groups in $L.i in $L.total.approvals)=0) then (current.pending.groups in $L.i in $L.total.approvals=delete(current.pending.groups in $L.i in $L.total.approvals, $L.j);$L.j=$L.j - 1))</statements>
      <statements sctype="string"/>
      <statements sctype="operator">$L.current.pending.seq=nullsub($L.min.current.seq, current.pending.seq in 1 in $L.total.approvals)</statements>
