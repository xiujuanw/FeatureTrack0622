      <statements sctype="operator">$L.scsFlg2=rtecall("rinit", $L.rtnCod, $L.upginfoBfr, "upginfo")</statements>
      <statements sctype="string"/>
      <statements sctype="operator">$L.rem=" Retrieve the upginfo record "</statements>
      <statements sctype="operator">$L.rem=" This record contains the languages selected when the upgrade was built, hence the laguages available for upgrading "</statements>
      <statements sctype="operator">$L.upgrade.to.pd=jscall("upgradePDMigration.isUpgradeToPD")</statements>
      <statements sctype="operator">$L.upginfoQryVar="pd.enabled="+$L.upgrade.to.pd</statements>
      <statements sctype="operator">if $L.scsFlg2 then ($L.scsFlg3=rtecall("select", $L.rtnCod3, $L.upginfoBfr, $L.upginfoQryVar))</statements>
      <statements sctype="string"/>
      <statements sctype="operator">$L.rem=" Populate the lang.selected array of structure in the upgradestatus record ONLY when these conditions exist: "</statements>
      <statements sctype="operator">$L.rem="   . The language is available in the upgrade (ie, the selected flag in the lang.selected array in upginfo is true) "</statements>
      <statements sctype="operator">$L.rem="   . The language is active on the customer's system "</statements>
      <statements sctype="string"/>
      <statements sctype="operator">$L.idxO=1</statements>
      <statements sctype="operator">$L.redo=false</statements>
      <statements sctype="operator">if ($L.rtnCod3=1) then for $L.idxI = 1 to lng(denull(lang.selected in $L.upginfoBfr)) do (if (1 in $L.idxI in lang.selected in $L.upginfoBfr=true) then ($L.rem=" Even though the language was included in the upgrade, determine if that language is active on the customer's system. ";if (not  ($L.redo)) then ($L.wrkLanCod=3 in $L.idxI in lang.selected in $L.upginfoBfr) else ($L.redo=false);$L.languageQryVar="unique.id=\""+$L.wrkLanCod+"\"";$L.scsFlg=rtecall("select", $L.rtnCod, $L.languageBfr, $L.languageQryVar);$L.rem=" Display that language as upgradeable ONLY if it is active on the customer's system. ";if ($L.scsFlg=true and $L.rtnCod~=3 and active in $L.languageBfr=true and jscall("upgradeLanguage.isLanguageApplied", unique.id in $L.languageBfr)) then ($L.idxO in lang.selected in $L.file=$L.idxI in lang.selected in $L.upginfoBfr;1 in $L.idxO in lang.selected in $L.file=true;$L.idxO+=1) else (if ($L.wrkLanCod="pt-Br") then ($L.wrkLanCod="pt";$L.redo=true;$L.idxI=$L.idxI - 1))))</statements>
