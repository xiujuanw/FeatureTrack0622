      <statements sctype="operator">for $L.i = 1 to lng(denull(secondary in $L.patch)) do ($L.i in $L.patch.dbdict=1 in $L.i in secondary in $L.patch;$L.i in $L.patch.add.only=nullsub(5 in $L.i in secondary in $L.patch, false))</statements>
