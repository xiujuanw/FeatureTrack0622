    <statements sctype="array">
      <statements sctype="operator">if $G.DbgAppTrc then ($L.stsMsg="*** Upgrade Status: apm.upgrade.status.graph.bg started. ***";$L.scsFlg=rtecall("log", $L.rtnCod, $L.stsMsg))</statements>
      <statements sctype="expression"/>
      <statements sctype="operator">$L.step=nullsub($4, "Initial Purge")</statements>
      <statements sctype="operator">$L.percent=round(nullsub($5, 0), 2);$L.send=true</statements>
      <statements sctype="operator">$L.upgrade.steps={"Converting Incident Data", "Converting Change Data"}</statements>
      <statements sctype="operator">$L.upgrade.weight={25, 20};$L.total.weight=0;$L.overall=0</statements>
      <statements sctype="operator">$L.pointer=index($L.step, $L.upgrade.steps);if ($L.pointer=0) then ($L.pointer=1;$L.send=false)</statements>
      <statements sctype="operator">for $L.i = 1 to lng($L.upgrade.weight) do ($L.total.weight+=($L.i in $L.upgrade.weight);if ($L.i&lt;$L.pointer) then $L.overall+=($L.i in $L.upgrade.weight))</statements>
      <statements sctype="operator">$L.overall.percent=$L.overall/$L.total.weight*100</statements>
      <statements sctype="operator">$L.this.weight=$L.pointer in $L.upgrade.weight</statements>
      <statements sctype="operator">$L.this.percent=$L.this.weight/$L.total.weight</statements>
      <statements sctype="operator">$L.overall.total=round(nullsub($L.overall.percent, 0)+$L.percent*$L.this.percent, 0);if ($L.overall.total&gt;99 and $L.percent&lt;100) then ($L.overall.total=99)</statements>
      <statements sctype="operator">$L.msg=str($L.percent)+";"+str(round($L.overall.total, 1))</statements>
      <statements sctype="operator">$L.totalstatusmsg="$SYSPUB.totalstatusbg;"+nullsub($L.msg, "0,0")</statements>
      <statements sctype="expression"/>
      <statements sctype="operator">$L.total.msg=scmsg(261, "upg", {round($L.overall.total, 0), "%"})</statements>
