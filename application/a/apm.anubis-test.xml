      <statements sctype="operator">for $L.x = 1 to lng(denull($L.list)) do (if (1 in 2 in $L.x in $L.list)#"-" then ($L.void=strclpl(1 in 2 in $L.x in $L.temp.list, 1)))</statements>
      <statements sctype="operator">for $L.x = 1 to lng(denull($L.processes)) do (if (4 in $L.x in $L.processes=2 in 2 in $L.i in $L.temp.list or 4 in $L.x in $L.processes=1 in 2 in $L.i in $L.temp.list) then ($L.hit=true))</statements>
