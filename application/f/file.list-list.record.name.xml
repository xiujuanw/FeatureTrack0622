      <statements sctype="operator">if (name in $dbdict="upgrade") then ($L.rec.name=nullsub(release in $file.variable, "")+", "+nullsub(module in $file.variable, "")+", "+nullsub(name in $file.variable, "")+", "+nullsub(type in $file.variable, "")+", "+nullsub(lookup in $file.variable, ""))</statements>
