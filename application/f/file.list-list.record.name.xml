      <statements sctype="operator">if (name in $dbdict="schedule") then ($L.rec.name=nullsub(class in $file.variable, "")+", "+nullsub(name in $file.variable, "")+", "+nullsub(application in $file.variable, ""))</statements>
      <statements sctype="operator">if (name in $dbdict="enclapplrev") then ($L.rec.name=application.name in $file.variable)</statements>
      <statements sctype="operator">if (name in $dbdict="format") then ($L.rec.name=name in $file.variable;if (not  (null(syslanguage in $file.variable))) then $L.rec.name+=(" ("+syslanguage in $file.variable+")"))</statements>
      <statements sctype="operator">if (name in $dbdict="SYSATTACHMENTS") then ($L.rec.name=application in $file.variable+", "+topic in $file.variable)</statements>
