      <statements sctype="operator">// Processing for the format/htmltemplates tables.</statements>
      <statements sctype="operator">if $L.old.table isin {"format", "htmltemplates"} then ($L.rem="Isolate the language code - the language code of the these record is the last two characters of the object.name field.";$L.lanCod=substr(object.name in $L.object, lng(denull(object.name in $L.object)) - 1, 2);if same($L.lanCod, "ns") then ($L.lanCod="zh-Hans") else (if same($L.lanCod, "Br") then ($L.lanCod="pt-Br"));if (index($L.lanCod, $L.selected.languages)&gt;0) then ($L.prcLan=true))</statements>
