      <expressions sctype="operator">$L.fldnambfr=1 in $L.idx in $L.data;$L.fldlvlbfr=2 in $L.idx in $L.data;$L.fldidxbfr=3 in $L.idx in $L.data;$L.fldtypbfr=4 in $L.idx in $L.data</expressions>
