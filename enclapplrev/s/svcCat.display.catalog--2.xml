          <rev.desc sctype="string"/>
          <rev.desc sctype="string">add the following statement</rev.desc>
          <rev.desc sctype="string">if (not null($search.string) and $search.string~="") then ($L.catalog.sql=$L.catalog.sql+" and displayName=\""+$search.string+"\"")</rev.desc>
          <rev.desc sctype="string">if (not null($search.category) and $noncategory=false) then if ($L.type="noncart") then ($L.catalog.sql=jscall("svcCatCategory.buildCatQuery", str($search.category), true, true)+" and "+$L.catalog.sql) else ($L.catalog.sql=jscall("svcCatCategory.buildCatQuery", str($search.category), true, false)+" and "+$L.catalog.sql)</rev.desc>
