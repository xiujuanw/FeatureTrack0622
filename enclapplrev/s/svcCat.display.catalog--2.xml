      <descript sctype="string">remove $L.sort.array from panel 'start'</descript>
      <descript sctype="string">remove $L.void=setsort($L.catalog, $L.sort, 0) from panel 'get.items' in RAD 'svcCat.display.catalog'</descript>
      <descript sctype="string">add new sort field '2 in $L.sort' into panel 'get.catalog.items' in RAD svcCat.display.catalog</descript>
