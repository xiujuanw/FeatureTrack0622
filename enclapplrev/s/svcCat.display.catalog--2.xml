      <descript sctype="string">setup.one.click</descript>
      <descript sctype="string">add statement:</descript>
      <descript sctype="string">if (not  (null($L.cart.item))) then if same(one.click in $L.file, true) then (svc.options in $L.file=options in $L.cart.item)</descript>
      <descript sctype="string">setup.cart</descript>
      <descript sctype="string"/>
      <descript sctype="string">$L.clientpid=sysinfo.get("ClientPID")</descript>
      <descript sctype="string">$L.clienttid=jscall("svcCartHelper.getClientTIDFromPID", $L.clientpid)</descript>
      <descript sctype="string">to</descript>
      <descript sctype="string">$L.clienttid=sysinfo.get("ThreadID")</descript>
      <descript sctype="string">Issue with Service Catalog search functionality</descript>
      <descript sctype="string">extract the build input string query to a JS function from RAD, so customer could tailoring it.</descript>
      <descript sctype="string">function :svcCatCategory.buildSearchQuery</descript>
      <descript sctype="string"/>
      <descript sctype="string">function buildSearchQuery(searchStr){</descript>
      <descript sctype="string">return  " displayName=\""+ searchStr+ "\"" ;</descript>
      <descript sctype="string">}</descript>
      <descript sctype="string"/>
      <descript sctype="string">RAD: svcCat.display.catalog &amp; Panel: set.search</descript>
      <descript sctype="string">change :</descript>
      <descript sctype="string">$L.catalog.sql =$L.catalog.sql + " and displayName=\""+ $search.string +"\""</descript>
      <descript sctype="string">to:</descript>
      <descript sctype="string">$L.catalog.sql =$L.catalog.sql +" and "+ jscall( "svcCatCategory.buildSearchQuery", str( $search.string))</descript>
      <descript sctype="string">checked in by yuli</descript>
      <descript sctype="string"/>
      <descript sctype="string">Search functionality in Service Catalog cannot find all items or bundles in all subcategories</descript>
      <descript sctype="string"/>
      <descript sctype="string">update panel: set.search</descript>
      <descript sctype="string"/>
      <descript sctype="string">add the following statement</descript>
      <descript sctype="string">if (not null($search.string) and $search.string~="") then ($L.catalog.sql=$L.catalog.sql+" and displayName=\""+$search.string+"\"")</descript>
      <descript sctype="string">if (not null($search.category) and $noncategory=false) then if ($L.type="noncart") then ($L.catalog.sql=jscall("svcCatCategory.buildCatQuery", str($search.category), true, true)+" and "+$L.catalog.sql) else ($L.catalog.sql=jscall("svcCatCategory.buildCatQuery", str($search.category), true, false)+" and "+$L.catalog.sql)</descript>
      <descript sctype="string">if (not null($search.category) and $noncategory=true) then if ($L.type="noncart") then ($L.catalog.sql=jscall("svcCatCategory.buildCatQuery", str($search.category), false, true)+" and "+$L.catalog.sql) else ($L.catalog.sql=jscall("svcCatCategory.buildCatQuery", str($search.category), false, false)+" and "+$L.catalog.sql)</descript>
      <descript sctype="string">Request to have an option to show Items only when searching in "Order from Category"</descript>
      <descript sctype="string">panel "start"</descript>
      <descript sctype="string">$noncategory=false</descript>
      <descript sctype="string">panel "set.search"</descript>
      <descript sctype="string">add the following statement</descript>
      <descript sctype="string">if ($noncategory=true) then ($L.catalog.sql=$L.catalog.sql+" and type~=\"category\"")</descript>
      <descript sctype="string">panel "cleanup"</descript>
      <descript sctype="string">cleanup($noncategory)</descript>
      <descript sctype="string">When searching in "Order from Category" query, use "and" instead of "or"</descript>
      <descript sctype="string">panel "start"</descript>
      <descript sctype="string">$irspread=3</descript>
      <descript sctype="string">panel "set.search"</descript>
      <descript sctype="string">add the following statement</descript>
      <descript sctype="string">$L.void=irspread($irspread)</descript>
