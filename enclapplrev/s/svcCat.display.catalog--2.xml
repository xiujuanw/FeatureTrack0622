      <rev.array sctype="structure">
        <rev.name type="string">zhenqing</rev.name>
        <rev.date type="dateTime">11/03/13 20:40:55</rev.date>
        <rev.desc sctype="array">
          <rev.desc sctype="string">Issue with Service Catalog search functionality</rev.desc>
          <rev.desc sctype="string">extract the build input string query to a JS function from RAD, so customer could tailoring it.</rev.desc>
          <rev.desc sctype="string">function :svcCatCategory.buildSearchQuery</rev.desc>
          <rev.desc sctype="string"/>
          <rev.desc sctype="string">function buildSearchQuery(searchStr){</rev.desc>
          <rev.desc sctype="string">return  " displayName=\""+ searchStr+ "\"" ;</rev.desc>
          <rev.desc sctype="string">}</rev.desc>
          <rev.desc sctype="string"/>
          <rev.desc sctype="string">RAD: svcCat.display.catalog &amp; Panel: set.search</rev.desc>
          <rev.desc sctype="string">change :</rev.desc>
          <rev.desc sctype="string">$L.catalog.sql =$L.catalog.sql + " and displayName=\""+ $search.string +"\""</rev.desc>
          <rev.desc sctype="string">to:</rev.desc>
          <rev.desc sctype="string">$L.catalog.sql =$L.catalog.sql +" and "+ jscall( "svcCatCategory.buildSearchQuery", str( $search.string))</rev.desc>
        </rev.desc>
        <change.no type="decimal">71576</change.no>
      </rev.array>
      <rev.array sctype="structure">
        <rev.name type="string">falcon</rev.name>
