      <descript sctype="string">$L.junk=jscall("svcCartHelper.clearDeadCartItemsByOperator", $L.svc.operator)</descript>
      <descript sctype="string">3.Update code of panel “process.svcCatProfile”:</descript>
      <descript sctype="string">From:</descript>
