    <descript sctype="array">
      <descript sctype="string">add $L.sort.types, support both asc and dsc sort.</descript>
      <descript sctype="string">locallize menus. add the following items</descript>
      <descript sctype="string">$ssd.opt1=scmsg(1, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt2=scmsg(2, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt3=scmsg(3, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt4=scmsg(4, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt5=scmsg(5, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt6=scmsg(6, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt7=scmsg(7, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt8=scmsg(8, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt9=scmsg(9, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt10=scmsg(10, "systemstatus")</descript>
      <descript sctype="string">$ssd.opt11=scmsg(11, "systemstatus")</descript>
      <descript sctype="string">"System Status" page is closed unexpectedly when inputing the special characters into "Go to page" text field.</descript>
