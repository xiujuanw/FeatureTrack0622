          <rev.desc sctype="string">exit: check.delect.old.attr</rev.desc>
          <rev.desc sctype="string">condition:  $L.delete=false and $L.add=false</rev.desc>
          <rev.desc sctype="string">Panel:  check.add</rev.desc>
          <rev.desc sctype="string">changed the normal exit from 'update.attr' to 'check.delete.old.attr'</rev.desc>
        </rev.desc>
        <change.no type="decimal">25779</change.no>
      </rev.array>
