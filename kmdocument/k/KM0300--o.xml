    <title type="string">UNABLE TO FIND OUTLOOK EXPRESS OR WINDOWS MAIL</title>
    <summary type="string">If unable to find Outlook Express or Windows Mail in MS Windows 7.</summary>
    <doctype type="string">probsol</doctype>
    <category type="string">probsol</category>
