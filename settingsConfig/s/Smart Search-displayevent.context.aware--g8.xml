<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="settingsConfig">
  <record id="setting=&quot;Smart Search&quot; and id=&quot;displayevent.context.aware&quot;" recordid="Smart Search - displayevent.context.aware">
    <setting type="string">Smart Search</setting>
    <id type="string">displayevent.context.aware</id>
    <label type="string">The displayevent that supports Smart Search actions</label>
    <type type="string">string</type>
    <addlTypeInfo NullValue="1" type="string"/>
    <displayType NullValue="1" type="string"/>
    <displayList NullValue="1" sctype="array">
      <displayList NullValue="1" type="string"/>
    </displayList>
    <valueList NullValue="1" sctype="array">
      <valueList NullValue="1" type="string"/>
    </valueList>
    <validationScript NullValue="1" type="string"/>
    <description type="string">Read-only. The default value is se.global_context_aware. Indicates the displayevent that is triggered to support Smart Search actions.</description>
    <mandatory type="boolean">true</mandatory>
    <settingType NullValue="1" type="string"/>
    <tableFieldName NullValue="1" type="string"/>
    <readonly type="boolean">true</readonly>
    <visible.condition type="string">index("smart.search.action.method", settingId in $L.file) in (settingValue in $L.file) = "displayevent"</visible.condition>
    <sysmodtime NullValue="1" type="dateTime"/>
    <sysmoduser NullValue="1" type="string"/>
    <sysmodcount NullValue="1" type="decimal"/>
  </record>
</recordset>
