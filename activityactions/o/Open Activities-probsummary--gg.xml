<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="activityactions">
  <record id="name=&quot;Open Activities&quot; and table=&quot;probsummary&quot;" recordid="Open Activities - probsummary">
    <name type="string">Open Activities</name>
    <condition type="operator">$L.mode="open" or $L.mode="add"</condition>
    <activities sctype="array">
      <activities sctype="structure">
        <activityname type="string">Open</activityname>
        <activitycondition type="operator">$L.mode="open" or $L.mode="add"</activitycondition>
        <activitydescription type="string">action in $L.file</activitydescription>
        <activitynotification NullValue="1" type="string"/>
        <bypassvisible NullValue="1" type="boolean"/>
      </activities>
      <activities sctype="structure">
        <activityname type="string">current.phase in $L.file</activityname>
        <activitycondition type="operator">problem.status in $L.file="Closed"</activitycondition>
        <activitydescription type="string">$pmc.actions</activitydescription>
        <activitynotification NullValue="1" type="string"/>
        <bypassvisible NullValue="1" type="boolean"/>
      </activities>
      <activities sctype="structure">
        <activityname type="string">Email Received</activityname>
        <activitycondition type="operator">($L.mode="open" or $L.mode="add") and nullsub($fromEmail, false)</activitycondition>
        <activitydescription type="string">{brief.description in $L.file}</activitydescription>
        <activitynotification NullValue="1" type="string"/>
        <bypassvisible NullValue="1" type="boolean"/>
      </activities>
    </activities>
    <mode NullValue="1" type="string"/>
    <table type="string">probsummary</table>
    <sysmodtime type="dateTime">06/21/16 17:22:14</sysmodtime>
    <sysmoduser type="string">falcon</sysmoduser>
    <sysmodcount type="decimal">11</sysmodcount>
  </record>
</recordset>
