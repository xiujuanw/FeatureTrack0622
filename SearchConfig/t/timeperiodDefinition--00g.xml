<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="SearchConfig">
  <record id="tablename=&quot;timeperiodDefinition&quot;" recordid="timeperiodDefinition">
    <tablename type="string">timeperiodDefinition</tablename>
    <choices sctype="array">
      <choices sctype="structure">
        <description type="string">Open</description>
        <id type="string">open</id>
        <query type="string">active=true</query>
      </choices>
      <choices sctype="structure">
        <description type="string">Retired</description>
        <id type="string">retired</id>
        <query type="string">active=false</query>
      </choices>
      <choices sctype="structure">
        <description type="string">Owned by me</description>
        <id type="string">owner</id>
        <query type="string">owner=operator()</query>
      </choices>
      <choices sctype="structure">
        <description type="string">Affecting all services</description>
        <id type="string">allservices</id>
        <query type="string">affected.services="all"</query>
      </choices>
    </choices>
    <searchFormat type="string">advFind.timeperiodDefinition.search</searchFormat>
    <range NullValue="1" sctype="array">
      <range sctype="structure">
        <fieldName NullValue="1" type="string"/>
        <operator1 NullValue="1" type="string"/>
        <variable1 NullValue="1" type="string"/>
        <specialType NullValue="1" type="string"/>
      </range>
    </range>
    <init.process NullValue="1" type="string"/>
    <allowAdvAccess type="operator">nullsub(jscall("security.getRights", "Timeperiod", "view"), "false")=true</allowAdvAccess>
    <sysmodtime NullValue="1" type="dateTime"/>
    <sysmoduser NullValue="1" type="string"/>
    <sysmodcount NullValue="1" type="decimal"/>
  </record>
</recordset>
