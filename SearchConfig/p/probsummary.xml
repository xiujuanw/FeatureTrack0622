<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="SearchConfig">
  <record id="tablename=&quot;probsummary&quot;" recordid="probsummary">
    <tablename type="string">probsummary</tablename>
    <choices sctype="array">
      <choices sctype="structure">
        <description type="string">Open</description>
        <id type="string">open</id>
        <query type="string">flag=true</query>
      </choices>
      <choices sctype="structure">
        <description type="string">Closed</description>
        <id type="string">closed</id>
        <query type="string">flag=false</query>
      </choices>
      <choices sctype="structure">
        <description type="string">Assigned to me</description>
        <id type="string">assigned</id>
        <query type="string">assignee.name=operator()</query>
      </choices>
      <choices sctype="structure">
        <description type="string">High Priority</description>
        <id type="string">highpriority</id>
        <query type="string">priority.code="1" or priority.code="2"</query>
      </choices>
      <choices sctype="structure">
        <description type="string">Total Loss of Service</description>
        <id type="string">tl</id>
        <query type="string">total.loss=true</query>
      </choices>
      <choices sctype="structure">
        <description type="string">Generated by UCMDB Integration</description>
        <id type="string">ucmdb</id>
        <query type="string">modEvent=true</query>
      </choices>
    </choices>
    <searchFormat type="string">im.advFind.incident.search</searchFormat>
    <range sctype="array">
      <range sctype="structure">
        <fieldName type="string">open.time</fieldName>
        <operator1 type="string">gte</operator1>
        <variable1 type="string">$adv.open.start</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">open.time</fieldName>
        <operator1 type="string">lte</operator1>
        <variable1 type="string">$adv.open.end</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">close.time</fieldName>
        <operator1 type="string">gte</operator1>
        <variable1 type="string">$adv.close.start</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">close.time</fieldName>
        <operator1 type="string">lte</operator1>
        <variable1 type="string">$adv.close.end</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">update.time</fieldName>
        <operator1 type="string">gte</operator1>
        <variable1 type="string">$pmc.update.start</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">update.time</fieldName>
        <operator1 type="string">lte</operator1>
        <variable1 type="string">$pmc.update.end</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">next.breach</fieldName>
        <operator1 type="string">gte</operator1>
        <variable1 type="string">$next.breach.start</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">next.breach</fieldName>
        <operator1 type="string">lte</operator1>
        <variable1 type="string">$next.breach.end</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">downtime.start</fieldName>
        <operator1 type="string">gte</operator1>
        <variable1 type="string">$down.time.start</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">downtime.start</fieldName>
        <operator1 type="string">lte</operator1>
        <variable1 type="string">$down.time.end</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">downtime.end</fieldName>
        <operator1 type="string">gte</operator1>
        <variable1 type="string">$down.end.start</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
      <range sctype="structure">
        <fieldName type="string">downtime.end</fieldName>
        <operator1 type="string">lte</operator1>
        <variable1 type="string">$down.end.end</variable1>
        <specialType NullValue="1" type="string"/>
      </range>
    </range>
    <init.process NullValue="1" type="string"/>
    <allowAdvAccess type="operator">browse in $G.pm.environment</allowAdvAccess>
    <sysmodtime NullValue="1" type="dateTime"/>
    <sysmoduser NullValue="1" type="string"/>
    <sysmodcount NullValue="1" type="decimal"/>
  </record>
</recordset>
