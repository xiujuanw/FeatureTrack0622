<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="SearchConfig">
  <record id="tablename=&quot;contract&quot;" recordid="contract">
    <tablename type="string">contract</tablename>
    <choices sctype="array">
      <choices sctype="structure">
        <description type="string">Contract Submitted</description>
        <id type="string">submittedby</id>
        <query type="string">submitted.by=operator()</query>
      </choices>
    </choices>
    <searchFormat type="string">advFind.search.contract</searchFormat>
    <range NullValue="1" sctype="array">
      <range sctype="structure">
        <fieldName NullValue="1" type="string"/>
        <operator1 NullValue="1" type="string"/>
        <variable1 NullValue="1" type="string"/>
        <specialType NullValue="1" type="string"/>
      </range>
    </range>
    <init.process NullValue="1" type="string"/>
    <allowAdvAccess type="operator">index("SysAdmin", $lo.ucapex)&gt;0 or nullsub(jscall("security.getRights", "Configuration Management Configuration", "view"), "false")="true"</allowAdvAccess>
    <sysmodtime NullValue="1" type="dateTime"/>
    <sysmoduser NullValue="1" type="string"/>
    <sysmodcount NullValue="1" type="decimal"/>
  </record>
</recordset>
