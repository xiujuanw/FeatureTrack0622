{
   var cityz=\"\";
   var loc = new SCFile(\"location\");
   var sql = \"location=\\\"\"+vars.$L_filed.ship_to_code+\"\\\"\";
   if (loc.doSelect(sql) == RC_SUCCESS)
   {
      cityz = loc.city + \"    \" + loc.state + \"   \" + loc.zip;
      var phone = loc.phone;
      var fax = loc.fax;
      //print(vars.$cityz);
   }
  
   if (cityz.indexOf(\"null\") != -1)
   {
      cityz = cityz.replace(/null/g,\" \");
   }
   if (phone == null) { phone = \" \"; }
   if (fax == null) { fax = \" \"; }
   vars.$cityz = cityz;
   vars.$phone = phone;
   vars.$fax = fax;
}", ]}</screen>
