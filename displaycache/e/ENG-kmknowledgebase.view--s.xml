kmknowledgebase.setFields(['kbname', 'kbdescription', 'kbtype', 'indexstatus', 'lastindextime', 'interval', 'spconnectorurl', 'sptask', 'idoltoken']);
var kbrc = kmknowledgebase.doSelect('kbtype &lt;&gt; \"sclib\"');
while (kbrc === RC_SUCCESS) {
