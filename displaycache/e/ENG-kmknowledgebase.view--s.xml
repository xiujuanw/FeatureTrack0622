      <ds.javascript.pre sctype="string">var record = vars['$L.file'];
lib.IDOLKMReindexSample.cleanupKMIndexRecords(record);

// TODO: Global List, use name-value on format ?
record.indexstatus = 'finished';

if (vars['$G.km.index.status.name'] &amp;&amp; vars['$G.km.index.status.name'].length() &gt; 0 &amp;&amp; 
    vars['$G.km.index.status.display'] &amp;&amp; vars['$G.km.index.status.display'].length() &gt; 0) {
  var index = lib.ArrayUtil.indexOf(vars['$G.km.index.status.name'], record.indexstatus);
  vars['$indexstatus'] = vars['$G.km.index.status.display'][index];
} else {
  vars['$indexstatus'] = record.indexstatus;
}

print(funcs.scmsg('force.stop.complete','idol',[ record.kbname ]));</ds.javascript.pre>
