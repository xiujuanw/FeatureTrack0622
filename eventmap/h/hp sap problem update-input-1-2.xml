    <evfile type="string">probsummary</evfile>
    <evfield type="string">hidden.meta.data</evfield>
    <evcalc sctype="array">
      <evcalc sctype="operator">$isIncidentExchangeFlag=not  (same("Requester:StartExchange:Error", $hmd))</evcalc>
      <evcalc sctype="operator">is.incident.exchange in $axces.target=$isIncidentExchangeFlag</evcalc>
      <evcalc sctype="operator">if ($hmd="" or index(":Error", $hmd)&gt;0) then (is.ictex.action.blocked in $axces.target=false)</evcalc>
      <evcalc sctype="string"/>
      <evcalc sctype="operator">if ($hmd="Closed") then (problem.status in $axces.target="Closed";current.phase in $axces.target="Closure";resolution.code in $axces.target="Automatically Closed";status in $axces.target="closed")</evcalc>
      <evcalc sctype="string"/>
      <evcalc sctype="operator">if ($hmd="Provider:SolutionProvided" or $hmd="Requester:SolutionProvided") then (problem.status in $axces.target="Resolved";status in $axces.target="updated";current.phase in $axces.target="Review")</evcalc>
      <evcalc sctype="operator">cleanup($isIncidentExchangeFlag);cleanup($hmd)</evcalc>
    </evcalc>
    <evtype type="string">input</evtype>
    <evsepchar NullValue="1" type="string"/>
    <evdtype type="decimal">2</evdtype>
