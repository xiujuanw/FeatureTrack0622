    <script.format type="string">cm3r.sla</script.format>
    <bypass.display type="boolean">true</bypass.display>
    <bypass.cond type="boolean">false</bypass.cond>
    <exits sctype="array">
      <exits sctype="string">cm3r.sla.01</exits>
