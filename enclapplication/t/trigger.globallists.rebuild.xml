  <record id="application.name=&quot;trigger.globallists.rebuild&quot;" recordid="trigger.globallists.rebuild">
    <application.name type="string">trigger.globallists.rebuild</application.name>
    <appl.desc sctype="array">
      <appl.desc sctype="string">RAD Application Name: trigger.globallists.rebuild</appl.desc>
      <appl.desc sctype="string">Description . . . . . . . . . : Expire a globallists record so that it is rebuilt on the next cycle by the lister background process</appl.desc>
      <appl.desc sctype="string"/>
      <appl.desc sctype="string">This application expires (ie, stamps the expiration field of the globallists record with the current date/time) so that it is rebuilt by the lister background process on its very next cycle of execution.</appl.desc>
      <appl.desc sctype="string"/>
      <appl.desc sctype="string">Parameters in to (I) or out from (O) this application are:</appl.desc>
      <appl.desc sctype="string"/>
      <appl.desc sctype="string">Parm             RAD                              Parm Name</appl.desc>
      <appl.desc sctype="string">Seq          Parm Type          I/O          (in this app)            Description</appl.desc>
      <appl.desc sctype="string"/>
      <appl.desc sctype="string">1              file                         I            $INTO.OLD           Snapshot of record before add/update/delete</appl.desc>
      <appl.desc sctype="string">2              record                   I            $INTO.NEW          Snapshot of record after add/update/delete</appl.desc>
      <appl.desc sctype="string">3              index                     I            $INTO.TYPE         Trigger Type</appl.desc>
      <appl.desc sctype="string">1=Before Add</appl.desc>
      <appl.desc sctype="string">2=After Add</appl.desc>
      <appl.desc sctype="string">3=Before Update</appl.desc>
      <appl.desc sctype="string">4=After Update</appl.desc>
      <appl.desc sctype="string">5=Before Delete</appl.desc>
      <appl.desc sctype="string">6=After Delete</appl.desc>
      <appl.desc sctype="string">4              number1               O           $ERROR.CODE    Error Code</appl.desc>
      <appl.desc sctype="string">5              name                    O           $ERROR.FIELD    Error Field</appl.desc>
    </appl.desc>
