    <sql type="string">CREATE VIEW V_SLA AS
SELECT t0.[AGREEMENT_ID],
        t0.[AGREEMENTS],
        t0.[CATEGORY],
        t0.[CURRENT_PHASE],
        t0.[CUSTOMER],
        t0.[DESCRIPTION],
        t0.[DISPLAY_CATEGORY],
        t0.[EXPIRATION],
        t0.[FOLDER],
        t0.[JOINVALUE],
        t0.[NEXT_REVIEW_DATE],
        t0.[OWNER],
        t0.[OWNER_GROUP],
        t0.[PENALTIES_INCENTIVES],
        t0.[REVIEW_DATE],
        t0.[REVIEW_FREQUENCY],
        t0.[SERVICE_CONTRACT],
        t0.[SERVICE_HOURS],
        t0.[START],
        t0.[SYSMODCOUNT],
        t0.[SYSMODTIME],
        t0.[SYSMODUSER],
        t0.[TITLE],
        t0.[TYPE],
        t0.[UPDATE_ACTION],
        t1.[AGREEMENT_ID] [SLARESPONSE_AGREEMENT_ID],
        t1.[DEVIATION],
        t1.[MEAN],
        t1.[MEDIAN],
        t1.[MONTH],
        t1.[PERHIT],
        t1.[RECORD_ID],
        t1.[RESPONSE_NAME],
        t1.[SLO_ID],
        t1.[SYSMODCOUNT] [SLARESPONSE_SYSMODCOUNT],
        t1.[SYSMODTIME] [SLARESPONSE_SYSMODTIME],
        t1.[SYSMODUSER] [SLARESPONSE_SYSMODUSER],
        t1.[TOTAL_BREACHED],
        t1.[TOTAL_RECORDS],
        t1.[WEIGHTED],
        t1.[YEAR] 
FROM SLAM1 t0
JOIN SLARESPONSEM1 t1 ON t0.[AGREEMENT_ID]=t1.[AGREEMENT_ID]</sql>
