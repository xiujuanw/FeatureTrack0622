    <sql type="string">CREATE VIEW V_SLM_AVAILABILITY AS
SELECT t0.[AGREEMENT_ID],
        t0.[AGREEMENTS],
        t0.[CATEGORY],
        t0.[CURRENT_PHASE],
        t0.[CUSTOMER],
        t0.[DESCRIPTION],
        t0.[DISPLAY_CATEGORY],
        t0.[EXPIRATION],
        t0.[FOLDER],
        t0.[JOINVALUE],
        t0.[NEXT_REVIEW_DATE],
        t0.[OWNER],
        t0.[OWNER_GROUP],
        t0.[PENALTIES_INCENTIVES],
        t0.[REVIEW_DATE],
        t0.[REVIEW_FREQUENCY],
        t0.[SERVICE_CONTRACT],
        t0.[SERVICE_HOURS],
        t0.[START],
        t0.[SYSMODCOUNT],
        t0.[SYSMODTIME],
        t0.[SYSMODUSER],
        t0.[TITLE],
        t0.[TYPE],
        t0.[UPDATE_ACTION],
        t1.[ACTUAL],
        t1.[AGREEMENT_CATEGORY],
        t1.[AGREEMENT_ID] [SLAMONTHLY_AGREEMENT_ID],
        t1.[AVAIL_TYPE],
        t1.[AVG_OUTAGE_DURATION],
        t1.[CLOSED],
        t1.[COST],
        t1.[COST_CURRENCY_CODE],
        t1.[DUR_PERCENTAGE],
        t1.[EXPECTED],
        t1.[LOGICAL_NAME],
        t1.[MAX_DUR_BREACHES],
        t1.[MONTH],
        t1.[MTBF],
        t1.[MTBF_ADJUSTED],
        t1.[NEXT_BREACH],
        t1.[PERCENTAGE],
        t1.[RESULT],
        t1.[ROOT_COST],
        t1.[ROOT_CURRENCY_CODE],
        t1.[SLO_ID],
        t1.[SLO_NAME],
        t1.[SYSMODCOUNT] [SLAMONTHLY_SYSMODCOUNT],
        t1.[SYSMODTIME] [SLAMONTHLY_SYSMODTIME],
        t1.[SYSMODUSER] [SLAMONTHLY_SYSMODUSER],
        t1.[TARGET],
        t1.[TOTAL_OUTAGES],
        t1.[WEIGHTED],
        t1.[YEAR] 
FROM SLAM1 t0
JOIN SLAMONTHLYM1 t1 ON t0.[AGREEMENT_ID]=t1.[AGREEMENT_ID]</sql>
