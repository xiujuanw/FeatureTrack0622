    <sql type="string">CREATE VIEW V_KNOWLEDGE_ACTIVITY AS
SELECT t0.[ANSWER],
        t0.[ASSIGNEDEDITOR],
        t0.[ASSOC_PUBLISHED_DOC],
        t0.[ASSOC_WC_DOC],
        t0.[AUTHOR],
        t0.[CAUSE],
        t0.[CHANGE_REQUEST_NUMBER],
        t0.[CREATION_DATE],
        t0.[DOCTYPE],
        t0.[DOCUMENTOWNER],
        t0.[DVDCONDITION],
        t0.[ERROR],
        t0.[EXPIRATION_DATE],
        t0.[FIX],
        t0.[HOTNEWS],
        t0.[HOTNEWS_END_DATE],
        t0.[HOTNEWS_START_DATE],
        t0.[ID],
        t0.[ID_ATTACH],
        t0.[LEGACYID],
        t0.[LEGAL_RIGHT],
        t0.[LEGISLATION],
        t0.[LOCALE],
        t0.[NOTIFY],
        t0.[PROBLEM],
        t0.[QUESTION],
        t0.[REASON],
        t0.[RECAUDIENCE],
        t0.[REFERENCE],
        t0.[SOLUTION],
        t0.[STATUS],
        t0.[SUBTYPE],
        t0.[SUMMARY],
        t0.[SYSMODCOUNT],
        t0.[SYSMODTIME],
        t0.[SYSMODUSER],
        t0.[TAGS],
        t0.[TITLE],
        t0.[USAGE_COUNT],
        t1.[CONTEXTTAG],
        t1.[ID] [KMUSAGEHISTORY_ID],
        t1.[KMID],
        t1.[SYSMODCOUNT] [KMUSAGEHISTORY_SYSMODCOUNT],
        t1.[SYSMODTIME] [KMUSAGEHISTORY_SYSMODTIME],
        t1.[SYSMODUSER] [KMUSAGEHISTORY_SYSMODUSER],
        t1.[TABLENAME],
        t1.[USAGETYPE],
        t1.[USERID] 
FROM KMDOCUMENTM1 t0
JOIN KMUSEHISM1 t1 ON t0.[ID]=t1.[KMID]</sql>
