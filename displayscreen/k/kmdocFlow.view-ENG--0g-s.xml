<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="displayscreen">
  <record id="screen.id=&quot;kmdocFlow.view&quot; and language=&quot;ENG&quot;" recordid="kmdocFlow.view - ENG">
    <screen.id type="string">kmdocFlow.view</screen.id>
    <screen.id.copy type="string">kmdocFlow.view</screen.id.copy>
    <language type="string">ENG</language>
    <title type="globalVar">$L.title</title>
    <form type="globalVar">$L.format</form>
    <io type="boolean">true</io>
    <time NullValue="1" type="dateTime"/>
    <initialization sctype="structure">
      <expressions sctype="array">
        <expressions sctype="operator">if sysinfo.get("environment")#"scguiwweb" then ($showWF="true") else ($showWF="false")</expressions>
        <expressions sctype="operator">if (not  (null($G.client.type))) then ($L.title=title in $L.file)</expressions>
        <expressions sctype="operator">$editAdaptive=evaluate(parse($G.editAdaptiveQuery, 4))</expressions>
      </expressions>
      <msg.condition NullValue="1" type="boolean"/>
      <message NullValue="1" type="string"/>
    </initialization>
    <screen.id.alias type="string">kmdocFlow.view</screen.id.alias>
    <on.enter.key type="string">return</on.enter.key>
    <window.structure sctype="structure">
      <open.window NullValue="1" type="boolean"/>
      <parent.name NullValue="1" type="string"/>
      <window.name NullValue="1" type="string"/>
      <window.position type="string">left</window.position>
      <window.percent NullValue="1" type="decimal"/>
      <window.lines NullValue="1" type="decimal"/>
    </window.structure>
    <screen.id.copy.old type="string">kmdocFlow.view</screen.id.copy.old>
    <more.window NullValue="1" type="boolean"/>
    <user.options type="boolean">true</user.options>
    <sysmodcount type="decimal">16</sysmodcount>
    <sysmoduser type="string">falcon</sysmoduser>
    <sysmodtime type="dateTime">08/31/20 05:16:23</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript type="string">vars.$recordType = null;

vars["$L.docwithview"] = lib.EndUserChatAutomation.setOOParameterInfo(vars["$L.docwithview"],vars["$L.file"])</javascript>
    <search.only NullValue="1" type="boolean"/>
  </record>
</recordset>
