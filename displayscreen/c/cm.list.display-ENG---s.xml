<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="displayscreen">
  <record id="screen.id=&quot;cm.list.display&quot; and language=&quot;ENG&quot;" recordid="cm.list.display - ENG">
    <screen.id type="string">cm.list.display</screen.id>
    <screen.id.copy type="string">cm.list.display</screen.id.copy>
    <language type="string">ENG</language>
    <title type="globalVar">$L.title</title>
    <form type="globalVar">$L.format</form>
    <io NullValue="1" type="boolean"/>
    <time NullValue="1" type="dateTime"/>
    <initialization sctype="structure">
      <expressions sctype="array">
        <expressions sctype="operator">if (filename($L.filed)="cm3r") then ($L.title=scmsg(807, "cm3"));if (filename($L.filed)="cm3t") then ($L.title=scmsg(808, "cm3"))</expressions>
      </expressions>
      <msg.condition NullValue="1" type="boolean"/>
      <message NullValue="1" type="string"/>
    </initialization>
    <screen.id.alias type="string">cm.list.display</screen.id.alias>
    <on.enter.key type="string">return</on.enter.key>
    <window.structure sctype="structure">
      <open.window type="boolean">false</open.window>
      <parent.name type="string">Main Window</parent.name>
      <window.name type="string">List of Changes</window.name>
      <window.position type="string">top</window.position>
      <window.percent type="decimal">500</window.percent>
      <window.lines NullValue="1" type="decimal"/>
    </window.structure>
    <screen.id.copy.old type="string">cm.list.display</screen.id.copy.old>
    <more.window NullValue="1" type="boolean"/>
    <user.options NullValue="1" type="boolean"/>
    <sysmodcount type="decimal">6</sysmodcount>
    <sysmoduser type="string">shiji.shu</sysmoduser>
    <sysmodtime type="dateTime">09/18/13 14:02:55</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript NullValue="1" type="string"/>
    <search.only NullValue="1" type="boolean"/>
  </record>
</recordset>
