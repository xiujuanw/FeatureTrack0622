<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="displayscreen">
  <record id="screen.id=&quot;task.view&quot; and language=&quot;ENG&quot;" recordid="task.view - ENG">
    <screen.id type="string">task.view</screen.id>
    <screen.id.copy type="string">task.view</screen.id.copy>
    <language type="string">ENG</language>
    <title type="globalVar">$L.title</title>
    <form type="globalVar">$L.format</form>
    <io type="boolean">true</io>
    <time NullValue="1" type="dateTime"/>
    <initialization sctype="structure">
      <expressions sctype="array">
        <expressions sctype="operator">if $G.pd.request.enabled then ($L.format="patcotask.pd")</expressions>
      </expressions>
      <msg.condition NullValue="1" type="boolean"/>
      <message NullValue="1" type="string"/>
    </initialization>
    <screen.id.alias type="string">task.view</screen.id.alias>
    <on.enter.key type="string">return</on.enter.key>
    <window.structure sctype="structure">
      <open.window NullValue="1" type="boolean"/>
      <parent.name NullValue="1" type="string"/>
      <window.name NullValue="1" type="string"/>
      <window.position type="string">left</window.position>
      <window.percent NullValue="1" type="decimal"/>
      <window.lines NullValue="1" type="decimal"/>
    </window.structure>
    <screen.id.copy.old type="string">task.view</screen.id.copy.old>
    <more.window NullValue="1" type="boolean"/>
    <user.options type="boolean">true</user.options>
    <sysmodcount type="decimal">6</sysmodcount>
    <sysmoduser type="string">falcon</sysmoduser>
    <sysmodtime type="dateTime">11/09/14 17:38:44</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript NullValue="1" type="string"/>
    <search.only NullValue="1" type="boolean"/>
  </record>
</recordset>
