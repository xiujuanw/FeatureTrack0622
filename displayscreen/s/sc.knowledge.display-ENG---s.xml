<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="displayscreen">
  <record id="screen.id=&quot;sc.knowledge.display&quot; and language=&quot;ENG&quot;" recordid="sc.knowledge.display - ENG">
    <screen.id type="string">sc.knowledge.display</screen.id>
    <screen.id.copy type="string">sc.knowledge.display</screen.id.copy>
    <language type="string">ENG</language>
    <title type="globalVar">$L.kb.title</title>
    <form type="globalVar">$L.ir.format</form>
    <io type="boolean">true</io>
    <time NullValue="1" type="dateTime"/>
    <initialization sctype="structure">
      <expressions sctype="array">
        <expressions sctype="operator">if (filename($L.filed)="rootcause") then ($L.mode="browse";$L.journal=nullsub(journal.pm in $G.rc.global.environment, false))</expressions>
      </expressions>
      <msg.condition NullValue="1" type="boolean"/>
      <message NullValue="1" type="string"/>
    </initialization>
    <screen.id.alias type="string">sc.knowledge.display</screen.id.alias>
    <on.enter.key type="string">do nothing</on.enter.key>
    <window.structure sctype="structure">
      <open.window NullValue="1" type="boolean"/>
      <parent.name NullValue="1" type="string"/>
      <window.name NullValue="1" type="string"/>
      <window.position type="string">left</window.position>
      <window.percent NullValue="1" type="decimal"/>
      <window.lines NullValue="1" type="decimal"/>
    </window.structure>
    <screen.id.copy.old type="string">sc.knowledge.display</screen.id.copy.old>
    <more.window NullValue="1" type="boolean"/>
    <user.options NullValue="1" type="boolean"/>
    <sysmodcount type="decimal">4</sysmodcount>
    <sysmoduser type="string">falcon</sysmoduser>
    <sysmodtime type="dateTime">01/11/07 12:54:06</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript NullValue="1" type="string"/>
    <search.only NullValue="1" type="boolean"/>
  </record>
</recordset>
