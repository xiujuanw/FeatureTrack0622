      <expressions sctype="array">
        <expressions sctype="operator">$L.has.sub=false</expressions>
        <expressions sctype="operator">$L.label=scmsg($L.filename, "tablename")</expressions>
        <expressions sctype="operator">if (index("Queue.AutoRefresh", $lo.ucapex)&gt;0) then ($operator.Q.auto.refresh=true)</expressions>
        <expressions sctype="operator">$clientType=sysinfo.get("environment")</expressions>
      </expressions>
      <msg.condition NullValue="1" type="boolean"/>
      <message NullValue="1" type="string"/>
    </initialization>
    <screen.id.alias type="string">scm.advanced</screen.id.alias>
