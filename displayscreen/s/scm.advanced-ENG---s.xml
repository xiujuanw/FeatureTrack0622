    <screen.id.copy.old type="string">scm.advanced</screen.id.copy.old>
    <more.window NullValue="1" type="boolean"/>
    <user.options NullValue="1" type="boolean"/>
    <sysmodcount type="decimal">89</sysmodcount>
    <sysmoduser type="string">zhangqi</sysmoduser>
    <sysmodtime type="dateTime">04/02/16 07:06:27</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript type="string">vars['$converted.refresh.interval']=null;
if(!funcs._null(vars['$L.inbox.record'])){
    vars['$inbox.id']=vars['$L.inbox.record']['inbox.id'];

    if(vars['$operator.Q.auto.refresh'] &amp;&amp; vars['$clientType']!="scguiwswt" &amp;&amp; vars['$L.inbox.record']['dashboard.refresh.enable']){
        vars['$converted.refresh.interval']=parseInt("-"+lib.ViewAutoRefresh.getRefreshInterval(vars['$inbox.id']));
    }
}
</javascript>
    <search.only NullValue="1" type="boolean"/>
