<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="displayscreen">
  <record id="screen.id=&quot;svcCat.display.checkout&quot; and language=&quot;ENG&quot;" recordid="svcCat.display.checkout - ENG">
    <screen.id type="string">svcCat.display.checkout</screen.id>
    <screen.id.copy type="string">svcCat.display.checkout</screen.id.copy>
    <language type="string">ENG</language>
    <title type="globalVar">$L.title</title>
    <form type="globalVar">$L.format</form>
    <io type="boolean">true</io>
    <time NullValue="1" type="dateTime"/>
    <initialization sctype="structure">
      <expressions sctype="array">
        <expressions sctype="operator">for $L.i = 1 to $L.show do ($L.temp="$L.item."+$L.i+"=\""+strrep($L.i in $L.desc.list, "\"", "\\\"")+"\"";$L.void=evaluate(parse($L.temp, 11)))</expressions>
        <expressions sctype="operator">for $L.i = 1 to $L.show do ($L.temp="$L.id."+$L.i+"=\""+$L.i in $L.id.list+"\"";$L.void=evaluate(parse($L.temp, 11)))</expressions>
        <expressions sctype="operator">for $L.i = 1 to $L.show do ($L.temp="$L.qty."+$L.i+"=\""+$L.i in $L.qty.list+"\"";$L.void=evaluate(parse($L.temp, 11)))</expressions>
        <expressions sctype="operator">for $L.i = 1 to $L.show do ($L.temp="$L.req."+$L.i+"=\""+$L.i in $L.req.list+"\"";$L.void=evaluate(parse($L.temp, 11)))</expressions>
        <expressions sctype="operator">for $L.i = 1 to $L.show do ($L.temp="$L.cost."+$L.i+"=\""+$L.i in $L.cost.list+"\"";$L.void=evaluate(parse($L.temp, 11)))</expressions>
      </expressions>
      <msg.condition NullValue="1" type="boolean"/>
      <message NullValue="1" type="string"/>
    </initialization>
    <screen.id.alias type="string">svcCat.display.checkout</screen.id.alias>
    <on.enter.key type="string">do nothing</on.enter.key>
    <window.structure sctype="structure">
      <open.window NullValue="1" type="boolean"/>
      <parent.name NullValue="1" type="string"/>
      <window.name NullValue="1" type="string"/>
      <window.position type="string">left</window.position>
      <window.percent NullValue="1" type="decimal"/>
      <window.lines NullValue="1" type="decimal"/>
    </window.structure>
    <screen.id.copy.old type="string">svcCat.display.checkout</screen.id.copy.old>
    <more.window NullValue="1" type="boolean"/>
    <user.options NullValue="1" type="boolean"/>
    <sysmodcount type="decimal">3</sysmodcount>
    <sysmoduser type="string">falcon</sysmoduser>
    <sysmodtime type="dateTime">01/11/07 12:54:12</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript NullValue="1" type="string"/>
    <search.only NullValue="1" type="boolean"/>
  </record>
</recordset>
