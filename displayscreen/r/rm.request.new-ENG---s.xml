    <sysmodcount type="decimal">24</sysmodcount>
    <sysmoduser type="string">zhangqi</sysmoduser>
    <sysmodtime type="dateTime">08/16/15 11:59:38</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript type="string">if(funcs.exists("workflow",vars.$L_file)==true &amp;&amp; vars.$L_file.workflow!=null)
{
    var fname = lib.dbdictHelper.getPrimaryTablename(vars.$L_file);
    
    var obj = new SCFile("Object", SCFILE_READONLY);
    obj.setFields(["file.name"]);
    if(obj.doSelect("category.file.name=\""+fname+"\"")==RC_SUCCESS)
    {
        vars.$L_wfgWFName=vars.$L_file.workflow;
        vars.$L_wfgWFTable=obj.file_name;   
    }
}

lib.RequestService.initSvcCartUserOptionInfo();

vars.$request_caption = funcs.scmsg("Request","object");

vars.$recordType="";
</javascript>
    <search.only NullValue="1" type="boolean"/>
  </record>
