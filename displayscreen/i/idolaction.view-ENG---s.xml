<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="displayscreen">
  <record id="screen.id=&quot;idolaction.view&quot; and language=&quot;ENG&quot;" recordid="idolaction.view - ENG">
    <screen.id type="string">idolaction.view</screen.id>
    <screen.id.copy type="string">idolaction.view</screen.id.copy>
    <language type="string">ENG</language>
    <title type="globalVar">$L.title</title>
    <form type="globalVar">$L.format</form>
    <io type="boolean">true</io>
    <time NullValue="1" type="dateTime"/>
    <initialization sctype="structure">
      <expressions sctype="array">
        <expressions sctype="operator">$L.title=nullsub($hp.script.title, nullsub($L.title, "IDOL Action"))</expressions>
        <expressions sctype="operator">$L.format="idolaction"</expressions>
        <expressions sctype="operator">$adapter.page.start=nullsub($adapter.page.start, 0)</expressions>
        <expressions sctype="operator">$adapter.current.page=nullsub($adapter.current.page, 1)</expressions>
        <expressions sctype="operator">$adapter.page.size=10</expressions>
        <expressions sctype="operator">$index.page.start=nullsub($index.page.start, 0)</expressions>
        <expressions sctype="operator">$index.current.page=nullsub($index.current.page, 1)</expressions>
        <expressions sctype="operator">$index.page.size=5</expressions>
      </expressions>
      <msg.condition NullValue="1" type="boolean"/>
      <message NullValue="1" type="string"/>
    </initialization>
    <screen.id.alias type="string">idolaction.view</screen.id.alias>
    <on.enter.key type="string">return</on.enter.key>
    <window.structure sctype="structure">
      <open.window NullValue="1" type="boolean"/>
      <parent.name NullValue="1" type="string"/>
      <window.name NullValue="1" type="string"/>
      <window.position type="string">left</window.position>
      <window.percent NullValue="1" type="decimal"/>
      <window.lines NullValue="1" type="decimal"/>
    </window.structure>
    <screen.id.copy.old type="string">idolaction.view</screen.id.copy.old>
    <more.window NullValue="1" type="boolean"/>
    <user.options NullValue="1" type="boolean"/>
    <sysmodcount type="decimal">32</sysmodcount>
    <sysmoduser type="string">pengfei</sysmoduser>
    <sysmodtime type="dateTime">04/23/14 15:38:48</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript type="string">lib.IDOL_Utilities.getTrainingStatus()
lib.IDOL_Utilities.getTestStatus()
lib.IDOL_Utilities.getIndexStatus()</javascript>
    <search.only NullValue="1" type="boolean"/>
  </record>
</recordset>
