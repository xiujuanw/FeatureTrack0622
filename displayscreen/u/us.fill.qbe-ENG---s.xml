<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="displayscreen">
  <record id="screen.id=&quot;us.fill.qbe&quot; and language=&quot;ENG&quot;" recordid="us.fill.qbe - ENG">
    <screen.id type="string">us.fill.qbe</screen.id>
    <screen.id.copy type="string">us.fill.qbe</screen.id.copy>
    <language type="string">ENG</language>
    <title type="globalVar">$L.title</title>
    <form type="globalVar">$L.qbe.format</form>
    <io type="boolean">true</io>
    <time NullValue="1" type="dateTime"/>
    <initialization sctype="structure">
      <expressions sctype="array">
        <expressions sctype="operator">$L.qbe.msg=scmsg(1817, "us", {filename($L.target), str($L.field)})</expressions>
        <expressions sctype="operator">$L.title=scmsg(1980, "us")</expressions>
        <expressions sctype="operator">$L.title.target=scmsg(19801, "us", {scmsg(filename($L.target), "tablename")})</expressions>
      </expressions>
      <msg.condition type="boolean">false</msg.condition>
      <message type="globalVar">$L.qbe.msg</message>
    </initialization>
    <screen.id.alias type="string">us.fill.qbe</screen.id.alias>
    <on.enter.key type="string">return</on.enter.key>
    <window.structure sctype="structure">
      <open.window type="boolean">true</open.window>
      <parent.name type="operator">current.window()</parent.name>
      <window.name type="globalVar">$L.title</window.name>
      <window.position type="string">centre</window.position>
      <window.percent type="decimal">10</window.percent>
      <window.lines NullValue="1" type="decimal"/>
    </window.structure>
    <screen.id.copy.old type="string">us.fill.qbe</screen.id.copy.old>
    <more.window NullValue="1" type="boolean"/>
    <user.options NullValue="1" type="boolean"/>
    <sysmodcount type="decimal">20</sysmodcount>
    <sysmoduser type="string">hanrui</sysmoduser>
    <sysmodtime type="dateTime">06/24/15 18:47:53</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript type="string">var titleTarget=vars['$L.title.target'];
      if (titleTarget&amp;&amp;titleTarget.indexOf("Could not be found") == -1) {
        vars['$L.title']=vars['$L.title.target'];
      }</javascript>
    <search.only NullValue="1" type="boolean"/>
  </record>
</recordset>
