    <javascript type="string">// value list &amp; display list shown in the format
vars.$L_channel_list = [];
vars.$L_channel_list_display = [];

var channels = lib.MicrosoftTeamsChannel.getAllActiveChannels();
channels.sort(function(a, b) {
    return (a.fullname ? a.fullname.localeCompare(b.fullname) : -1);
});

// add empty channel id first
vars.$L_channel_list.push(lib.MicrosoftTeamsConstants.EMPTY_CHANNELID());
vars.$L_channel_list_display.push(funcs.scmsg("21", "msteams"));

channels.forEach(function(channel) {
    vars.$L_channel_list.push(channel.id);
    vars.$L_channel_list_display.push(channel.fullname);
});</javascript>
