    <screen.id.copy.old type="string">main.mobile</screen.id.copy.old>
    <more.window NullValue="1" type="boolean"/>
    <user.options NullValue="1" type="boolean"/>
    <sysmodcount type="decimal">0</sysmodcount>
    <sysmoduser type="string">xuejun</sysmoduser>
    <sysmodtime type="dateTime">03/17/13 17:37:45</sysmodtime>
    <object.related NullValue="1" type="boolean"/>
    <javascript NullValue="1" type="string"/>
    <search.only NullValue="1" type="boolean"/>
  </record>
</recordset>
