<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="displayscreen">
  <record id="screen.id=&quot;main.mobile&quot; and language=&quot;ENG&quot;" recordid="main.mobile - ENG">
    <screen.id type="string">main.mobile</screen.id>
    <screen.id.copy type="string">main.mobile</screen.id.copy>
    <language type="string">ENG</language>
    <title type="globalVar">$L.title</title>
    <form type="globalVar">$L.format</form>
    <io NullValue="1" type="boolean"/>
    <time NullValue="1" type="dateTime"/>
    <initialization sctype="structure">
      <expressions sctype="array">
        <expressions sctype="operator">$L.has.sub=false</expressions>
        <expressions sctype="operator">$L.label=scmsg($L.filename, "tablename")</expressions>
      </expressions>
      <msg.condition NullValue="1" type="boolean"/>
      <message NullValue="1" type="string"/>
    </initialization>
    <screen.id.alias type="string">main.mobile</screen.id.alias>
    <on.enter.key type="string">return</on.enter.key>
