<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="agent">
  <record id="name=&quot;alert&quot;" recordid="alert">
    <name type="string">alert</name>
    <chart.flag type="boolean">true</chart.flag>
    <marquee.flag type="boolean">true</marquee.flag>
    <query.names sctype="array">
      <query.names sctype="string">alert1</query.names>
      <query.names sctype="string">alert2</query.names>
    </query.names>
    <marquees sctype="array">
      <marquees sctype="structure">
        <marquee.text type="operator">str(tod())+": There are "+1 in $L.results+" problems at Alert Stage 1"</marquee.text>
        <marquee.color type="globalVar">$L.color1</marquee.color>
      </marquees>
      <marquees sctype="structure">
        <marquee.text type="operator">str(tod())+": There are "+2 in $L.results+" problems at Alert Stage 2 "</marquee.text>
        <marquee.color type="globalVar">$L.color2</marquee.color>
      </marquees>
    </marquees>
    <expressions sctype="array">
      <expressions sctype="operator">$L.color1="White";if (val(1 in $L.results)&gt;3) then ($L.color1="Green");if (val(1 in $L.results)&gt;5) then ($L.color1="Yellow");if (val(1 in $L.results)&gt;10) then ($L.color1="Red")</expressions>
      <expressions sctype="operator">$L.color2="White";if (val(2 in $L.results)&gt;3) then ($L.color2="Green");if (val(2 in $L.results)&gt;5) then ($L.color2="Yellow");if (val(2 in $L.results)&gt;10) then ($L.color2="Red")</expressions>
    </expressions>
    <total type="boolean">false</total>
    <active type="boolean">false</active>
    <marquee.db.flg type="boolean">false</marquee.db.flg>
    <percent.flg type="boolean">false</percent.flg>
    <store NullValue="1" type="boolean"/>
    <stat.names NullValue="1" sctype="array">
      <stat.names NullValue="1" type="string"/>
    </stat.names>
    <sysmodcount type="decimal">2</sysmodcount>
    <sysmoduser type="string">sanderson</sysmoduser>
    <sysmodtime type="dateTime">06/02/08 08:35:17</sysmodtime>
  </record>
</recordset>
