<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="Process">
  <record id="process=&quot;idolserver.globalsearch&quot;" recordid="idolserver.globalsearch">
    <process type="string">idolserver.globalsearch</process>
    <pre.expressions sctype="array">
      <pre.expressions sctype="operator">$L.warning.msg=scmsg(68, "idol")</pre.expressions>
    </pre.expressions>
    <post.expressions NullValue="1" sctype="array">
      <post.expressions NullValue="1" type="expression"/>
    </post.expressions>
    <rad sctype="array">
      <rad sctype="structure">
        <application type="string">apm.mb.ok</application>
        <names sctype="array">
          <names sctype="string">text</names>
          <names sctype="string">index</names>
        </names>
        <values sctype="array">
          <values sctype="string">$L.warning.msg</values>
          <values sctype="string">2</values>
        </values>
        <rad.condition type="operator">not  ($L.idol.enable)</rad.condition>
        <pre.rad.expressions sctype="array"/>
        <post.rad.expressions sctype="array"/>
      </rad>
      <rad sctype="structure">
        <application type="string">se.view.engine</application>
        <names sctype="array">
          <names sctype="string">file</names>
          <names sctype="string">prompt</names>
          <names sctype="string">boolean1</names>
        </names>
        <values sctype="array">
          <values sctype="string">$L.km.env</values>
          <values sctype="string">"environment.km.idol.g"</values>
          <values sctype="string">false</values>
        </values>
        <rad.condition type="globalVar">$L.idol.enable</rad.condition>
        <pre.rad.expressions sctype="string"/>
        <post.rad.expressions sctype="string"/>
      </rad>
    </rad>
    <next.process NullValue="1" sctype="array">
      <next.process NullValue="1" type="string"/>
    </next.process>
    <process.condition NullValue="1" sctype="array">
      <process.condition NullValue="1" type="boolean"/>
    </process.condition>
    <run.standard NullValue="1" type="boolean"/>
    <sysmodcount type="decimal">15</sysmodcount>
    <sysmoduser type="string">falcon</sysmoduser>
    <sysmodtime type="dateTime">12/18/18 13:02:14</sysmodtime>
    <save.cursor.position NullValue="1" type="boolean"/>
    <run.in.window NullValue="1" type="boolean"/>
    <window.name NullValue="1" type="string"/>
    <javascript.pre type="string">var query ='name="knowledge management"';
vars['$L.km.env']=new SCFile('environment');
vars['$L.km.env'].doSelect(query);
vars.$L_idol_enable = vars["$lo.idol.enabled"];</javascript.pre>
    <javascript.post NullValue="1" type="string"/>
    <baseMethod NullValue="1" type="string"/>
  </record>
</recordset>
