    </post.expressions>
    <rad sctype="array">
      <rad sctype="structure">
        <application type="string">rml.close</application>
        <names sctype="array">
          <names sctype="string">file</names>
          <names sctype="string">second.file</names>
          <names sctype="string">text</names>
          <names sctype="string">boolean1</names>
