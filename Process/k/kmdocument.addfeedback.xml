    <window.name NullValue="1" type="string"/>
    <javascript.pre type="string">if (vars.$L_kmcreateWC=="true")
{
	print(system.functions.scmsg(116, "km")); // Enter feedback about why this document needs to modified or Skip Feedback.
}

print(vars.$L_fbfile.kmdocumentid);

var scfile = new SCFile('kmfeedback');
var ret = scfile.doSelect('kmdocumentid=\"' + vars.$L_fbfile.kmdocumentid + '\"');
print(RCtoString(ret))

var voteUps = 0, voteDowns = 0;
while (ret == RC_SUCCESS) {
	if (scfile.useful === true) voteUps++;
	else if (scfile.useful === false) voteDowns++;
	ret = scfile.getNext();
}

// set to $kmfdstatistics
vars.$kmfdstatistics = system.functions.scmsg("1","kmfeedback",[voteUps, voteDowns]);
</javascript.pre>
    <javascript.post NullValue="1" type="string"/>
