        <pre.rad.expressions sctype="array"/>
        <post.rad.expressions sctype="array">
          <post.rad.expressions sctype="operator">if ($L.continue=true) then ($L.temp.sql="id=\""+id in $L.file+"\"")</post.rad.expressions>
