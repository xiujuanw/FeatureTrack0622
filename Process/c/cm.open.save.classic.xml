        <rad.condition type="operator">not  (null(logical.name in $L.file)) and filename($L.file)="cm3r" and $L.exit="normal"</rad.condition>
        <pre.rad.expressions sctype="array">
          <pre.rad.expressions sctype="operator">$L.sch.filename="cm3r";$L.qry.num=number in $L.file</pre.rad.expressions>
        </pre.rad.expressions>
