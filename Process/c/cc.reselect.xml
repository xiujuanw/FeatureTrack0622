<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="Process">
  <record id="process=&quot;cc.reselect&quot;" recordid="cc.reselect">
    <process type="string">cc.reselect</process>
    <pre.expressions sctype="array">
      <pre.expressions sctype="operator">$L.file.contents=contents($L.file)</pre.expressions>
      <pre.expressions sctype="operator">$L.id=incident.id in $L.file</pre.expressions>
      <pre.expressions sctype="operator">$L.temp.sql="incident.id=\""+$L.id+"\""</pre.expressions>
      <pre.expressions sctype="operator">$L.success=rtecall("select", $L.return.code, $L.file, $L.temp.sql, {}, false)</pre.expressions>
      <pre.expressions sctype="operator">if ($L.success and $L.return.code=1) then ($L.sql=$L.temp.sql;$L.exit="resetrec") else (contents($L.file)=$L.file.contents)</pre.expressions>
    </pre.expressions>
    <post.expressions NullValue="1" sctype="array">
      <post.expressions NullValue="1" type="expression"/>
    </post.expressions>
    <rad NullValue="1" sctype="array">
      <rad sctype="structure">
        <application NullValue="1" type="string"/>
        <names NullValue="1" sctype="array">
          <names NullValue="1" type="string"/>
        </names>
        <values NullValue="1" sctype="array">
          <values NullValue="1" type="string"/>
        </values>
        <rad.condition NullValue="1" type="boolean"/>
        <pre.rad.expressions NullValue="1" sctype="array">
          <pre.rad.expressions NullValue="1" type="expression"/>
        </pre.rad.expressions>
        <post.rad.expressions NullValue="1" sctype="array">
          <post.rad.expressions NullValue="1" type="expression"/>
        </post.rad.expressions>
      </rad>
    </rad>
    <next.process NullValue="1" sctype="array">
      <next.process NullValue="1" type="string"/>
    </next.process>
    <process.condition NullValue="1" sctype="array">
      <process.condition NullValue="1" type="boolean"/>
    </process.condition>
    <run.standard NullValue="1" type="boolean"/>
    <sysmodcount NullValue="1" type="decimal"/>
    <sysmoduser NullValue="1" type="string"/>
    <sysmodtime NullValue="1" type="dateTime"/>
    <save.cursor.position NullValue="1" type="boolean"/>
    <run.in.window NullValue="1" type="boolean"/>
    <window.name NullValue="1" type="string"/>
    <javascript.pre NullValue="1" type="string"/>
    <javascript.post NullValue="1" type="string"/>
    <baseMethod NullValue="1" type="string"/>
  </record>
</recordset>
