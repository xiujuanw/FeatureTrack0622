			    vars.$fieldCaptions= system.functions.insert(vars.$fieldCaptions, 1, 1, convertCaption);
			    vars.$fieldNames   = system.functions.insert(vars.$fieldNames, 1, 1, convertFieldName);

			}
	    }
	}
	
	var sortArray=new SCDatum([vars.$fieldCaptions, vars.$fieldNames]);
	 
	system.functions.rtecall("sort", 1, sortArray, 0, 0);
	vars.$fieldCaptions=sortArray[0];
	vars.$fieldNames=sortArray[1];
