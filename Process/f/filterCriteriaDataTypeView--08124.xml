	if(str==null){
		str = "";
	}
	var i1 = str.lastIndexOf(';');
	
	if (i1&gt;0){
	    var i2 = str.lastIndexOf(';', i1-1);
	    if(i2&gt;=0){
	        vars.$L_fieldName = str.substr(i1+1);
