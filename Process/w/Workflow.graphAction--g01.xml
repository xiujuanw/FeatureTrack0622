      <pre.expressions sctype="operator">$L.temp.names={"$L.wf.action", "$L.wf.target", "$L.subform", "$L.phasename", "$L.info", "$L.description", "$L.displayForm", "$L.onEnter", "$L.onExit", "$L.onUpdate", "$L.phase.names", "$L.phase.desc"}</pre.expressions>
