          <values sctype="string">"true"</values>
          <values sctype="string">"name"</values>
          <values sctype="string">$resp.list</values>
        </values>
        <rad.condition type="operator">not  (null($L.slamodulecontrol)) and nullsub(use.phases in $L.slamodulecontrol, false)=true and not  (null(phase.file.name in $L.obj))</rad.condition>
        <pre.rad.expressions sctype="array"/>
        <post.rad.expressions sctype="array">
          <post.rad.expressions sctype="operator">if (index(initial.state in $L.file, $resp.list)&lt;=0) then (initial.state in $L.file=NULL)</post.rad.expressions>
          <post.rad.expressions sctype="operator">if (index(final.state in $L.file, $resp.list)&lt;=0) then (final.state in $L.file=NULL)</post.rad.expressions>
          <post.rad.expressions sctype="operator">if (filename($L.file)="slocatalog" and not  (null($L.file.save)) and name in $L.file=name in $L.file.save and slo.type in $L.file=slo.type in $L.file.save and service.area in $L.file=service.area in $L.file.save) then (initial.state in $L.file=initial.state in $L.file.save;final.state in $L.file=final.state in $L.file.save)</post.rad.expressions>
          <post.rad.expressions sctype="operator">if (not  (null($L.slamodulecontrol)) and nullsub(use.phases in $L.slamodulecontrol, false)=true and not  (null(phase.file.name in $L.obj))) then ($resp.list.disp=jscall("sloDisplay.getLocalizedWfPhase", file.name in $L.obj, $resp.list))</post.rad.expressions>
