          <values sctype="string">$L.slamodulecontrol</values>
          <values sctype="string">true</values>
          <values sctype="string">"table.name"</values>
          <values sctype="string">"slamodulecontrol"</values>
        </values>
        <rad.condition type="boolean">true</rad.condition>
        <pre.rad.expressions sctype="string"/>
        <post.rad.expressions sctype="array">
          <post.rad.expressions sctype="operator">if (not  (null($L.slamodulecontrol)) and nullsub(use.phases in $L.slamodulecontrol, false)=false) then ($resp.list=status.progression in $L.slamodulecontrol;$resp.list.disp=jscall("sloDisplay.getLocalizedModuleStatus", table.name in $L.slamodulecontrol, $resp.list))</post.rad.expressions>
