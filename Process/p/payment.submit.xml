    <pre.expressions sctype="array">
      <pre.expressions sctype="operator">status in $L.file="submitted"</pre.expressions>
      <pre.expressions sctype="operator">date.submitted in $L.file=date(tod())</pre.expressions>
    </pre.expressions>
    <post.expressions NullValue="1" sctype="array">
      <post.expressions NullValue="1" type="expression"/>
    </post.expressions>
    <rad sctype="array">
      <rad sctype="structure">
        <application type="string">base.update.record.bg</application>
        <names sctype="array">
          <names sctype="string">record</names>
          <names sctype="string">prompt</names>
        </names>
        <values sctype="array">
          <values sctype="string">$L.file</values>
          <values sctype="string">$L.action</values>
        </values>
        <rad.condition type="boolean">true</rad.condition>
        <pre.rad.expressions sctype="array">
          <pre.rad.expressions sctype="operator">$L.action="update"</pre.rad.expressions>
        </pre.rad.expressions>
        <post.rad.expressions sctype="array"/>
