<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<recordset table="Process">
  <record id="process=&quot;upgrade.wizard.apply.execute&quot;" recordid="upgrade.wizard.apply.execute">
    <process type="string">upgrade.wizard.apply.execute</process>
    <pre.expressions NullValue="1" sctype="array">
      <pre.expressions NullValue="1" type="expression"/>
    </pre.expressions>
    <post.expressions NullValue="1" sctype="array">
      <post.expressions NullValue="1" type="expression"/>
    </post.expressions>
    <rad sctype="array">
      <rad sctype="structure">
        <application type="string">apm.upgrade.apply.execute</application>
        <names sctype="array">
          <names sctype="string">boolean1</names>
        </names>
        <values sctype="array">
          <values sctype="string">false</values>
        </values>
        <rad.condition type="operator">$mode~#"Generate"</rad.condition>
        <pre.rad.expressions sctype="array"/>
        <post.rad.expressions sctype="array">
          <post.rad.expressions sctype="operator">if $mode#"Generate" then ($L.status.rec=jscall("upgradeUtility.getRecord", "upgradestatus", "true"))</post.rad.expressions>
        </post.rad.expressions>
      </rad>
      <rad sctype="structure">
        <application type="string">apm.upgrade.load.inf</application>
        <names sctype="array">
          <names sctype="string">string1</names>
        </names>
        <values sctype="array">
          <values sctype="string">path in $L.status.rec</values>
        </values>
        <rad.condition type="operator">$mode#"Generate"</rad.condition>
        <pre.rad.expressions sctype="array"/>
        <post.rad.expressions sctype="array">
          <post.rad.expressions sctype="operator">if $mode#"Generate" then ($L.void=jscall("upgradeBaseXmlGenerater.generate", path in $L.status.rec))</post.rad.expressions>
        </post.rad.expressions>
      </rad>
    </rad>
    <next.process NullValue="1" sctype="array">
      <next.process NullValue="1" type="string"/>
    </next.process>
    <process.condition NullValue="1" sctype="array">
      <process.condition NullValue="1" type="boolean"/>
    </process.condition>
    <run.standard NullValue="1" type="boolean"/>
    <sysmodcount type="decimal">1</sysmodcount>
    <sysmoduser type="string">zhouanqing</sysmoduser>
    <sysmodtime type="dateTime">11/16/20 19:22:23</sysmodtime>
    <save.cursor.position NullValue="1" type="boolean"/>
    <run.in.window NullValue="1" type="boolean"/>
    <window.name NullValue="1" type="string"/>
    <javascript.pre NullValue="1" type="string"/>
    <javascript.post NullValue="1" type="string"/>
    <baseMethod NullValue="1" type="string"/>
  </record>
</recordset>
